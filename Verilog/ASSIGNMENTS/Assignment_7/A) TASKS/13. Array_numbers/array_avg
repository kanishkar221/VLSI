#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffe1a0b510 .scope module, "array_avg" "array_avg" 2 1;
 .timescale 0 0;
v0x7fffe1a58370 .array/i "arr", 4 0, 31 0;
v0x7fffe1a58450_0 .var/i "avg", 31 0;
S_0x7fffe1a0b6a0 .scope task, "calc_average" "calc_average" 2 6, 2 6 0, S_0x7fffe1a0b510;
 .timescale 0 0;
v0x7fffe1a33e50_0 .var/i "j", 31 0;
v0x7fffe1a581d0_0 .var/i "result", 31 0;
v0x7fffe1a582b0_0 .var/i "sum", 31 0;
TD_array_avg.calc_average ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fffe1a582b0_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fffe1a33e50_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x7fffe1a33e50_0;
    %cmpi/s 5, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x7fffe1a582b0_0;
    %ix/getv/s 4, v0x7fffe1a33e50_0;
    %load/vec4a v0x7fffe1a58370, 4;
    %add;
    %store/vec4 v0x7fffe1a582b0_0, 0, 32;
    %load/vec4 v0x7fffe1a33e50_0;
    %addi 1, 0, 32;
    %store/vec4 v0x7fffe1a33e50_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %load/vec4 v0x7fffe1a582b0_0;
    %pushi/vec4 5, 0, 32;
    %div/s;
    %store/vec4 v0x7fffe1a581d0_0, 0, 32;
    %end;
    .scope S_0x7fffe1a0b510;
T_1 ;
    %pushi/vec4 10, 0, 32;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x7fffe1a58370, 4, 0;
    %pushi/vec4 20, 0, 32;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x7fffe1a58370, 4, 0;
    %pushi/vec4 30, 0, 32;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x7fffe1a58370, 4, 0;
    %pushi/vec4 40, 0, 32;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x7fffe1a58370, 4, 0;
    %pushi/vec4 50, 0, 32;
    %ix/load 4, 4, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x7fffe1a58370, 4, 0;
    %fork TD_array_avg.calc_average, S_0x7fffe1a0b6a0;
    %join;
    %load/vec4 v0x7fffe1a581d0_0;
    %store/vec4 v0x7fffe1a58450_0, 0, 32;
    %vpi_call 2 27 "$display", "Array Elements = %0d %0d %0d %0d %0d", &A<v0x7fffe1a58370, 0>, &A<v0x7fffe1a58370, 1>, &A<v0x7fffe1a58370, 2>, &A<v0x7fffe1a58370, 3>, &A<v0x7fffe1a58370, 4> {0 0 0};
    %vpi_call 2 28 "$display", "Average = %0d", v0x7fffe1a58450_0 {0 0 0};
    %delay 10, 0;
    %vpi_call 2 29 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "array_avg.v";

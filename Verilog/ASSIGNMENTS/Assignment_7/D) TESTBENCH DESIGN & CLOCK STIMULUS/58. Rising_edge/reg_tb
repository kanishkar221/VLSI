#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffcebc2540 .scope module, "reg_tb" "reg_tb" 2 1;
 .timescale 0 0;
v0x7fffcebc26f0_0 .var "clk", 0 0;
v0x7fffcec0bae0_0 .var "d", 0 0;
v0x7fffcec0bba0_0 .var "q", 0 0;
E_0x7fffcebc2910 .event posedge, v0x7fffcebc26f0_0;
    .scope S_0x7fffcebc2540;
T_0 ;
    %wait E_0x7fffcebc2910;
    %load/vec4 v0x7fffcec0bae0_0;
    %assign/vec4 v0x7fffcec0bba0_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x7fffcebc2540;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffcebc26f0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x7fffcebc2540;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x7fffcebc26f0_0;
    %inv;
    %store/vec4 v0x7fffcebc26f0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x7fffcebc2540;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffcec0bae0_0, 0, 1;
    %vpi_call 2 14 "$monitor", "t=%0t clk=%b d=%b q=%b", $time, v0x7fffcebc26f0_0, v0x7fffcec0bae0_0, v0x7fffcec0bba0_0 {0 0 0};
    %delay 8, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffcec0bae0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffcec0bae0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffcec0bae0_0, 0, 1;
    %delay 20, 0;
    %vpi_call 2 18 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "reg_tb.v";

#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffeb541110 .scope module, "up_down_counter_tb" "up_down_counter_tb" 2 1;
 .timescale 0 0;
v0x7fffeb554f50_0 .var "clk", 0 0;
v0x7fffeb555010_0 .net "count", 3 0, v0x7fffeb554c30_0;  1 drivers
v0x7fffeb5550e0_0 .var "reset", 0 0;
v0x7fffeb5551e0_0 .var "up_down", 0 0;
S_0x7fffeb5412a0 .scope module, "uut" "up_down_counter" 2 6, 3 1 0, S_0x7fffeb541110;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "up_down";
    .port_info 3 /OUTPUT 4 "count";
v0x7fffeb507490_0 .net "clk", 0 0, v0x7fffeb554f50_0;  1 drivers
v0x7fffeb554c30_0 .var "count", 3 0;
v0x7fffeb554d10_0 .net "reset", 0 0, v0x7fffeb5550e0_0;  1 drivers
v0x7fffeb554de0_0 .net "up_down", 0 0, v0x7fffeb5551e0_0;  1 drivers
E_0x7fffeb5086a0 .event posedge, v0x7fffeb554d10_0, v0x7fffeb507490_0;
    .scope S_0x7fffeb5412a0;
T_0 ;
    %wait E_0x7fffeb5086a0;
    %load/vec4 v0x7fffeb554d10_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x7fffeb554c30_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x7fffeb554de0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x7fffeb554c30_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x7fffeb554c30_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x7fffeb554c30_0;
    %subi 1, 0, 4;
    %assign/vec4 v0x7fffeb554c30_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x7fffeb541110;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffeb554f50_0, 0, 1;
T_1.0 ;
    %delay 5, 0;
    %load/vec4 v0x7fffeb554f50_0;
    %inv;
    %store/vec4 v0x7fffeb554f50_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x7fffeb541110;
T_2 ;
    %vpi_call 2 19 "$dumpfile", "up_down_counter.vcd" {0 0 0};
    %vpi_call 2 20 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x7fffeb541110 {0 0 0};
    %vpi_call 2 22 "$monitor", "Time=%0t | reset=%b | up_down=%b | count=%b", $time, v0x7fffeb5550e0_0, v0x7fffeb5551e0_0, v0x7fffeb555010_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffeb5550e0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffeb5551e0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffeb5550e0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffeb5551e0_0, 0, 1;
    %delay 80, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffeb5551e0_0, 0, 1;
    %delay 80, 0;
    %vpi_call 2 30 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "up_down_counter_tb.v";
    "up_down_counter.v";

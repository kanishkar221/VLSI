S_0x7fffd1bb3540 .scope module, "signed_add" "signed_add" 2 1;
 .timescale 0 0;
v0x7fffd1c00680_0 .var/s "a", 7 0;
v0x7fffd1c00780_0 .var/s "b", 7 0;
v0x7fffd1c00860_0 .var/s "sum", 8 0;
S_0x7fffd1bb36d0 .scope task, "add_task" "add_task" 2 5, 2 5 0, S_0x7fffd1bb3540;
 .timescale 0 0;
v0x7fffd1bdbce0_0 .var/s "s", 8 0;
v0x7fffd1c004e0_0 .var/s "x", 7 0;
v0x7fffd1c005c0_0 .var/s "y", 7 0;
TD_signed_add.add_task ;
    %load/vec4 v0x7fffd1c004e0_0;
    %pad/s 9;
    %load/vec4 v0x7fffd1c005c0_0;
    %pad/s 9;
    %add;
    %store/vec4 v0x7fffd1bdbce0_0, 0, 9;
    %end;
    .scope S_0x7fffd1bb3540;
T_1 ;
    %vpi_call 2 14 "$monitor", "Time=%0t | a=%d | b=%d | sum=%d", $time, v0x7fffd1c00680_0, v0x7fffd1c00780_0, v0x7fffd1c00860_0 {0 0 0};
    %pushi/vec4 50, 0, 8;
    %store/vec4 v0x7fffd1c00680_0, 0, 8;
    %pushi/vec4 20, 0, 8;
    %store/vec4 v0x7fffd1c00780_0, 0, 8;
    %load/vec4 v0x7fffd1c00680_0;
    %store/vec4 v0x7fffd1c004e0_0, 0, 8;
    %load/vec4 v0x7fffd1c00780_0;
    %store/vec4 v0x7fffd1c005c0_0, 0, 8;
    %fork TD_signed_add.add_task, S_0x7fffd1bb36d0;
    %join;
    %load/vec4 v0x7fffd1bdbce0_0;
    %store/vec4 v0x7fffd1c00860_0, 0, 9;
    %delay 5, 0;
    %pushi/vec4 226, 0, 8;

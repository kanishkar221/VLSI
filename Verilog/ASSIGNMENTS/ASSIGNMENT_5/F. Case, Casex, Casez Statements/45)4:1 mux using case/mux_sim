    .port_info 0 /INPUT 2 "sel";
    .port_info 1 /INPUT 1 "a";
    .port_info 2 /INPUT 1 "b";
    .port_info 3 /INPUT 1 "c";
    .port_info 4 /INPUT 1 "d";
    .port_info 5 /OUTPUT 1 "y";
v0x7fffcbd55490_0 .net "a", 0 0, v0x7fffcbda3d70_0;  1 drivers
v0x7fffcbda38a0_0 .net "b", 0 0, v0x7fffcbda3e30_0;  1 drivers
v0x7fffcbda3960_0 .net "c", 0 0, v0x7fffcbda3f00_0;  1 drivers
v0x7fffcbda3a00_0 .net "d", 0 0, v0x7fffcbda4000_0;  1 drivers
v0x7fffcbda3ac0_0 .net "sel", 1 0, v0x7fffcbda40d0_0;  1 drivers
v0x7fffcbda3bf0_0 .var "y", 0 0;
E_0x7fffcbd55a70/0 .event anyedge, v0x7fffcbda3ac0_0, v0x7fffcbd55490_0, v0x7fffcbda38a0_0, v0x7fffcbda3960_0;
E_0x7fffcbd55a70/1 .event anyedge, v0x7fffcbda3a00_0;
E_0x7fffcbd55a70 .event/or E_0x7fffcbd55a70/0, E_0x7fffcbd55a70/1;
    .scope S_0x7fffcbd8fb20;
T_0 ;
    %wait E_0x7fffcbd55a70;
    %load/vec4 v0x7fffcbda3ac0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %pushi/vec4 1, 1, 1;
    %store/vec4 v0x7fffcbda3bf0_0, 0, 1;
    %jmp T_0.5;
T_0.0 ;
    %load/vec4 v0x7fffcbd55490_0;
    %store/vec4 v0x7fffcbda3bf0_0, 0, 1;
    %jmp T_0.5;
T_0.1 ;
    %load/vec4 v0x7fffcbda38a0_0;
    %store/vec4 v0x7fffcbda3bf0_0, 0, 1;
    %jmp T_0.5;
T_0.2 ;
    %load/vec4 v0x7fffcbda3960_0;
    %store/vec4 v0x7fffcbda3bf0_0, 0, 1;
    %jmp T_0.5;
T_0.3 ;
    %load/vec4 v0x7fffcbda3a00_0;
    %store/vec4 v0x7fffcbda3bf0_0, 0, 1;
    %jmp T_0.5;
T_0.5 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x7fffcbd8f990;
T_1 ;
    %vpi_call 2 16 "$monitor", "Time=%0t | sel=%b | a=%b b=%b c=%b d=%b | y=%b", $time, v0x7fffcbda40d0_0, v0x7fffcbda3d70_0, v0x7fffcbda3e30_0, v0x7fffcbda3f00_0, v0x7fffcbda4000_0, v0x7fffcbda41c0_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffcbda3d70_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffcbda3e30_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffcbda3f00_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffcbda4000_0, 0, 1;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x7fffcbda40d0_0, 0, 2;
    %delay 10, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x7fffcbda40d0_0, 0, 2;
    %delay 10, 0;

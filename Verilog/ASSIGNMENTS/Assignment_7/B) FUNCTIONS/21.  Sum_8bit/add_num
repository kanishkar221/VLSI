 .timescale 0 0;
; Variable add is vec4 return value of scope S_0x7fffc8748680
v0x7fffc8794de0_0 .var "x", 7 0;
v0x7fffc8794ec0_0 .var "y", 7 0;
TD_add_num.add ;

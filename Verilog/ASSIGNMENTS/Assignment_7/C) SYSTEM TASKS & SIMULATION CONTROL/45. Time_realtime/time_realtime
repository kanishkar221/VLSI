#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7ffff2668540 .scope module, "real_realtime" "real_realtime" 2 1;
 .timescale 0 0;
v0x7ffff26686d0_0 .var "clk", 0 0;
v0x7ffff26b0ff0_0 .var "r_time", 0 0;
    .scope S_0x7ffff2668540;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7ffff26686d0_0, 0, 1;
T_0.0 ;
    %delay 5, 0;
    %load/vec4 v0x7ffff26686d0_0;
    %inv;
    %store/vec4 v0x7ffff26686d0_0, 0, 1;
    %jmp T_0.0;
    %end;
    .thread T_0;
    .scope S_0x7ffff2668540;
T_1 ;
    %vpi_call 2 11 "$display", "Time(integer) | Realtime(real) | Clock" {0 0 0};
    %pushi/vec4 6, 0, 32;
T_1.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_1.1, 5;
    %jmp/1 T_1.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 3, 0;
    %vpi_func/r 2 15 "$realtime" {0 0 0};
    %cvt/vr 1;
    %store/vec4 v0x7ffff26b0ff0_0, 0, 1;
    %vpi_call 2 16 "$display", "Time=%0t | %0f | %b", $time, v0x7ffff26b0ff0_0, v0x7ffff26686d0_0 {0 0 0};
    %jmp T_1.0;
T_1.1 ;
    %pop/vec4 1;
    %vpi_call 2 18 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "time_realtime.v";

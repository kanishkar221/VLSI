; Variable even_parity is vec4 return value of scope S_0x7ffff229f490
v0x7ffff22ed2d0_0 .var/i "i", 31 0;
TD_parity_generator.even_parity ;
    %pushi/vec4 0, 0, 1;
    %ret/vec4 0, 0, 1;  Assign to even_parity (store_vec4_to_lval)
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7ffff22ed2d0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x7ffff22ed2d0_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.1, 5;
    %retload/vec4 0; Load even_parity (draw_signal_vec4)
    %load/vec4 v0x7ffff229f670_0;
    %load/vec4 v0x7ffff22ed2d0_0;
    %part/s 1;
    %xor;
    %ret/vec4 0, 0, 1;  Assign to even_parity (store_vec4_to_lval)
    %load/vec4 v0x7ffff22ed2d0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x7ffff22ed2d0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .scope S_0x7ffff22c8360;
T_1 ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x7ffff22ed390_0, 0, 8;

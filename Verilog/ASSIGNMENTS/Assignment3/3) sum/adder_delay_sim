    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x7fffe687d030_0, 0, 4;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x7fffe687d0f0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x7fffe687d030_0, 0, 4;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x7fffe687d0f0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 1, 0, 4;

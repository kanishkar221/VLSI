TD_adder_function.add_with_carry ;
    %load/vec4 v0x7fffef615c70_0;
    %pad/u 5;
    %load/vec4 v0x7fffef615d30_0;
    %pad/u 5;
    %add;
    %load/vec4 v0x7fffef615bb0_0;
    %pad/u 5;
    %add;
    %ret/vec4 0, 0, 5;  Assign to add_with_carry (store_vec4_to_lval)
    %end;
    .scope S_0x7fffef5cb490;
T_1 ;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x7fffef615e10_0, 0, 4;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x7fffef615f10_0, 0, 4;

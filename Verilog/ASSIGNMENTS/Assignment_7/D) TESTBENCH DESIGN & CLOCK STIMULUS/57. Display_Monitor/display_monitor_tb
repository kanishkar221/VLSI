#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffedad3510 .scope module, "display_monitor_tb" "display_monitor_tb" 2 1;
 .timescale 0 0;
L_0x7fffedb1d010 .functor AND 1, v0x7fffedad36a0_0, v0x7fffedb1ceb0_0, C4<1>, C4<1>;
v0x7fffedad36a0_0 .var "a", 0 0;
v0x7fffedb1ceb0_0 .var "b", 0 0;
v0x7fffedb1cf70_0 .net "y", 0 0, L_0x7fffedb1d010;  1 drivers
    .scope S_0x7fffedad3510;
T_0 ;
    %vpi_call 2 8 "$monitor", "MONITOR: t=%0t a=%b b=%b y=%b", $time, v0x7fffedad36a0_0, v0x7fffedb1ceb0_0, v0x7fffedb1cf70_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffedad36a0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffedb1ceb0_0, 0, 1;
    %delay 5, 0;
    %vpi_call 2 10 "$display", "DISPLAY: t=%0t a=%b b=%b y=%b", $time, v0x7fffedad36a0_0, v0x7fffedb1ceb0_0, v0x7fffedb1cf70_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffedad36a0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffedb1ceb0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffedad36a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffedb1ceb0_0, 0, 1;
    %delay 5, 0;
    %vpi_call 2 13 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "display_monitor_tb.v";

L_0x7fffe687d570 .delay 5 (5,5,5) L_0x7fffe687d570/d;
L_0x7fffe687d570/d .arith/sum 5, L_0x7fffe687d2c0, L_0x7fffe687d430;
    .scope S_0x7fffe6868630;
T_0 ;

#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffc71bd570 .scope module, "mult_display" "mult_display" 2 1;
 .timescale 0 0;
v0x7fffc720a240_0 .var "a", 7 0;
v0x7fffc720a340_0 .var "b", 7 0;
S_0x7fffc71bd700 .scope task, "multiply_task" "multiply_task" 2 4, 2 4 0, S_0x7fffc71bd570;
 .timescale 0 0;
v0x7fffc71e5920_0 .var "product", 15 0;
v0x7fffc720a0a0_0 .var "x", 7 0;
v0x7fffc720a180_0 .var "y", 7 0;
TD_mult_display.multiply_task ;
    %load/vec4 v0x7fffc720a0a0_0;
    %pad/u 16;
    %load/vec4 v0x7fffc720a180_0;
    %pad/u 16;
    %mul;
    %store/vec4 v0x7fffc71e5920_0, 0, 16;
    %vpi_call 2 9 "$display", "Time=%0t | a=%0d | b=%0d | product=%0d", $time, v0x7fffc720a0a0_0, v0x7fffc720a180_0, v0x7fffc71e5920_0 {0 0 0};
    %end;
    .scope S_0x7fffc71bd570;
T_1 ;
    %pushi/vec4 10, 0, 8;
    %store/vec4 v0x7fffc720a240_0, 0, 8;
    %pushi/vec4 5, 0, 8;
    %store/vec4 v0x7fffc720a340_0, 0, 8;
    %load/vec4 v0x7fffc720a240_0;
    %store/vec4 v0x7fffc720a0a0_0, 0, 8;
    %load/vec4 v0x7fffc720a340_0;
    %store/vec4 v0x7fffc720a180_0, 0, 8;
    %fork TD_mult_display.multiply_task, S_0x7fffc71bd700;
    %join;
    %delay 5, 0;
    %pushi/vec4 12, 0, 8;
    %store/vec4 v0x7fffc720a240_0, 0, 8;
    %pushi/vec4 12, 0, 8;
    %store/vec4 v0x7fffc720a340_0, 0, 8;
    %load/vec4 v0x7fffc720a240_0;
    %store/vec4 v0x7fffc720a0a0_0, 0, 8;
    %load/vec4 v0x7fffc720a340_0;
    %store/vec4 v0x7fffc720a180_0, 0, 8;
    %fork TD_mult_display.multiply_task, S_0x7fffc71bd700;
    %join;
    %delay 5, 0;
    %pushi/vec4 25, 0, 8;
    %store/vec4 v0x7fffc720a240_0, 0, 8;
    %pushi/vec4 4, 0, 8;
    %store/vec4 v0x7fffc720a340_0, 0, 8;
    %load/vec4 v0x7fffc720a240_0;
    %store/vec4 v0x7fffc720a0a0_0, 0, 8;
    %load/vec4 v0x7fffc720a340_0;
    %store/vec4 v0x7fffc720a180_0, 0, 8;
    %fork TD_mult_display.multiply_task, S_0x7fffc71bd700;
    %join;
    %delay 5, 0;
    %pushi/vec4 100, 0, 8;
    %store/vec4 v0x7fffc720a240_0, 0, 8;
    %pushi/vec4 3, 0, 8;
    %store/vec4 v0x7fffc720a340_0, 0, 8;
    %load/vec4 v0x7fffc720a240_0;
    %store/vec4 v0x7fffc720a0a0_0, 0, 8;
    %load/vec4 v0x7fffc720a340_0;
    %store/vec4 v0x7fffc720a180_0, 0, 8;
    %fork TD_mult_display.multiply_task, S_0x7fffc71bd700;
    %join;
    %delay 5, 0;
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0x7fffc720a240_0, 0, 8;
    %pushi/vec4 2, 0, 8;
    %store/vec4 v0x7fffc720a340_0, 0, 8;
    %load/vec4 v0x7fffc720a240_0;
    %store/vec4 v0x7fffc720a0a0_0, 0, 8;
    %load/vec4 v0x7fffc720a340_0;
    %store/vec4 v0x7fffc720a180_0, 0, 8;
    %fork TD_mult_display.multiply_task, S_0x7fffc71bd700;
    %join;
    %delay 5, 0;
    %delay 20, 0;
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;

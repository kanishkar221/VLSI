#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffd8542d50 .scope module, "async_ring_counter_tb" "async_ring_counter_tb" 2 1;
 .timescale 0 0;
v0x7fffd8556c60_0 .var "clk", 0 0;
v0x7fffd8556d30_0 .net "q", 3 0, L_0x7fffd8556f00;  1 drivers
v0x7fffd8556e00_0 .var "reset", 0 0;
S_0x7fffd8542ee0 .scope module, "uut" "async_ring_counter" 2 6, 3 1 0, S_0x7fffd8542d50;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 4 "q";
L_0x7fffd8556f00 .functor BUFZ 4, v0x7fffd8556a60_0, C4<0000>, C4<0000>, C4<0000>;
v0x7fffd8543130_0 .net "clk", 0 0, v0x7fffd8556c60_0;  1 drivers
v0x7fffd8556980_0 .net "q", 3 0, L_0x7fffd8556f00;  alias, 1 drivers
v0x7fffd8556a60_0 .var "q_reg", 3 0;
v0x7fffd8556b20_0 .net "reset", 0 0, v0x7fffd8556e00_0;  1 drivers
E_0x7fffd85096a0 .event posedge, v0x7fffd8556b20_0, v0x7fffd8543130_0;
    .scope S_0x7fffd8542ee0;
T_0 ;
    %wait E_0x7fffd85096a0;
    %load/vec4 v0x7fffd8556b20_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 8, 0, 4;
    %assign/vec4 v0x7fffd8556a60_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x7fffd8556a60_0;
    %parti/s 1, 0, 2;
    %ix/load 4, 3, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x7fffd8556a60_0, 4, 5;
    %load/vec4 v0x7fffd8556a60_0;
    %parti/s 1, 3, 3;
    %ix/load 4, 2, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x7fffd8556a60_0, 4, 5;
    %load/vec4 v0x7fffd8556a60_0;
    %parti/s 1, 2, 3;
    %ix/load 4, 1, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x7fffd8556a60_0, 4, 5;
    %load/vec4 v0x7fffd8556a60_0;
    %parti/s 1, 1, 2;
    %ix/load 4, 0, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x7fffd8556a60_0, 4, 5;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x7fffd8542d50;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffd8556c60_0, 0, 1;
T_1.0 ;
    %delay 5, 0;
    %load/vec4 v0x7fffd8556c60_0;
    %inv;
    %store/vec4 v0x7fffd8556c60_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x7fffd8542d50;
T_2 ;
    %vpi_call 2 18 "$dumpfile", "async_ring_counter.vcd" {0 0 0};
    %vpi_call 2 19 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x7fffd8542d50 {0 0 0};
    %vpi_call 2 21 "$monitor", "Time=%0t | reset=%b | q=%b", $time, v0x7fffd8556e00_0, v0x7fffd8556d30_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffd8556e00_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffd8556e00_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 27 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.

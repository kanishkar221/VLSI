#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffbecbf510 .scope module, "write_display" "write_display" 2 1;
 .timescale 0 0;
v0x7fffbecbf6a0_0 .net *"_ivl_0", 4 0, L_0x7fffbed0a500;  1 drivers
L_0x7f22a4800018 .functor BUFT 1, C4<0>, C4<0>, C4<0>, C4<0>;
v0x7fffbed09f90_0 .net *"_ivl_3", 0 0, L_0x7f22a4800018;  1 drivers
v0x7fffbed0a070_0 .net *"_ivl_4", 4 0, L_0x7fffbed0a670;  1 drivers
L_0x7f22a4800060 .functor BUFT 1, C4<0>, C4<0>, C4<0>, C4<0>;
v0x7fffbed0a130_0 .net *"_ivl_7", 0 0, L_0x7f22a4800060;  1 drivers
v0x7fffbed0a210_0 .var "a", 3 0;
v0x7fffbed0a340_0 .var "b", 3 0;
v0x7fffbed0a420_0 .net "sum", 4 0, L_0x7fffbed0a7c0;  1 drivers
L_0x7fffbed0a500 .concat [ 4 1 0 0], v0x7fffbed0a210_0, L_0x7f22a4800018;
L_0x7fffbed0a670 .concat [ 4 1 0 0], v0x7fffbed0a340_0, L_0x7f22a4800060;
L_0x7fffbed0a7c0 .arith/sum 5, L_0x7fffbed0a500, L_0x7fffbed0a670;
    .scope S_0x7fffbecbf510;
T_0 ;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x7fffbed0a210_0, 0, 4;
    %pushi/vec4 7, 0, 4;
    %store/vec4 v0x7fffbed0a340_0, 0, 4;
    %vpi_call 2 11 "$display", "Using $display :" {0 0 0};
    %vpi_call 2 12 "$display", "a=%0d, b=%0d, sum=%0d", v0x7fffbed0a210_0, v0x7fffbed0a340_0, v0x7fffbed0a420_0 {0 0 0};
    %vpi_call 2 14 "$display", "-------------------" {0 0 0};
    %vpi_call 2 16 "$display", "Using $write:" {0 0 0};
    %vpi_call 2 17 "$write", "a=%0d,", v0x7fffbed0a210_0 {0 0 0};
    %vpi_call 2 18 "$write", "b=%0d,", v0x7fffbed0a340_0 {0 0 0};
    %vpi_call 2 19 "$write", "sum =%0d", v0x7fffbed0a420_0 {0 0 0};
    %vpi_call 2 20 "$display", " <-- End of the line " {0 0 0};
    %vpi_call 2 22 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "write_display.v";

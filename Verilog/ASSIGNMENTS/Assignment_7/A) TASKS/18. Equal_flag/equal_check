 .timescale 0 0;
v0x7ffff64b2fa0_0 .var "a", 7 0;
v0x7ffff64b30a0_0 .var "b", 7 0;
v0x7ffff64b3180_0 .var "flag", 0 0;
S_0x7ffff6465720 .scope task, "check_equal" "check_equal" 2 5, 2 5 0, S_0x7ffff6465590;
 .timescale 0 0;
v0x7ffff648e0e0_0 .var "flag_out", 0 0;
v0x7ffff64b2e00_0 .var "x", 7 0;
v0x7ffff64b2ee0_0 .var "y", 7 0;
TD_equal_check.check_equal ;
    %load/vec4 v0x7ffff64b2e00_0;
    %load/vec4 v0x7ffff64b2ee0_0;
    %cmp/e;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7ffff648e0e0_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7ffff648e0e0_0, 0, 1;
T_0.1 ;
    %end;
    .scope S_0x7ffff6465590;
T_1 ;
    %vpi_call 2 17 "$monitor", "Time=%0t | a=%0d | b=%0d | flag(equal)=%b", $time, v0x7ffff64b2fa0_0, v0x7ffff64b30a0_0, v0x7ffff64b3180_0 {0 0 0};
    %pushi/vec4 25, 0, 8;
    %store/vec4 v0x7ffff64b2fa0_0, 0, 8;
    %pushi/vec4 25, 0, 8;
    %store/vec4 v0x7ffff64b30a0_0, 0, 8;
    %load/vec4 v0x7ffff64b2fa0_0;
    %store/vec4 v0x7ffff64b2e00_0, 0, 8;

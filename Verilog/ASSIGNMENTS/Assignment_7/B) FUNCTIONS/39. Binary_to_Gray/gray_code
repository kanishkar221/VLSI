#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffd0f5d530 .scope module, "gray_code" "gray_code" 2 1;
 .timescale 0 0;
v0x7fffd0faa120_0 .var "bin", 3 0;
v0x7fffd0faa220_0 .var "gray", 3 0;
S_0x7fffd0f5d6c0 .scope function.vec4.s4, "bin_to_gray" "bin_to_gray" 2 6, 2 6 0, S_0x7fffd0f5d530;
 .timescale 0 0;
v0x7fffd0f85660_0 .var "b", 3 0;
; Variable bin_to_gray is vec4 return value of scope S_0x7fffd0f5d6c0
TD_gray_code.bin_to_gray ;
    %load/vec4 v0x7fffd0f85660_0;
    %load/vec4 v0x7fffd0f85660_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftr 4;
    %xor;
    %ret/vec4 0, 0, 4;  Assign to bin_to_gray (store_vec4_to_lval)
    %end;
    .scope S_0x7fffd0f5d530;
T_1 ;
    %vpi_call 2 14 "$monitor", "Time=%0t | Binary=%b (%0d) | Gray=%b", $time, v0x7fffd0faa120_0, v0x7fffd0faa120_0, v0x7fffd0faa220_0 {0 0 0};
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x7fffd0faa120_0, 0, 4;
    %load/vec4 v0x7fffd0faa120_0;
    %store/vec4 v0x7fffd0f85660_0, 0, 4;
    %callf/vec4 TD_gray_code.bin_to_gray, S_0x7fffd0f5d6c0;
    %store/vec4 v0x7fffd0faa220_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x7fffd0faa120_0, 0, 4;
    %load/vec4 v0x7fffd0faa120_0;
    %store/vec4 v0x7fffd0f85660_0, 0, 4;
    %callf/vec4 TD_gray_code.bin_to_gray, S_0x7fffd0f5d6c0;
    %store/vec4 v0x7fffd0faa220_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x7fffd0faa120_0, 0, 4;
    %load/vec4 v0x7fffd0faa120_0;
    %store/vec4 v0x7fffd0f85660_0, 0, 4;
    %callf/vec4 TD_gray_code.bin_to_gray, S_0x7fffd0f5d6c0;
    %store/vec4 v0x7fffd0faa220_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x7fffd0faa120_0, 0, 4;
    %load/vec4 v0x7fffd0faa120_0;
    %store/vec4 v0x7fffd0f85660_0, 0, 4;
    %callf/vec4 TD_gray_code.bin_to_gray, S_0x7fffd0f5d6c0;
    %store/vec4 v0x7fffd0faa220_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x7fffd0faa120_0, 0, 4;
    %load/vec4 v0x7fffd0faa120_0;
    %store/vec4 v0x7fffd0f85660_0, 0, 4;
    %callf/vec4 TD_gray_code.bin_to_gray, S_0x7fffd0f5d6c0;
    %store/vec4 v0x7fffd0faa220_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x7fffd0faa120_0, 0, 4;
    %load/vec4 v0x7fffd0faa120_0;
    %store/vec4 v0x7fffd0f85660_0, 0, 4;
    %callf/vec4 TD_gray_code.bin_to_gray, S_0x7fffd0f5d6c0;
    %store/vec4 v0x7fffd0faa220_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x7fffd0faa120_0, 0, 4;
    %load/vec4 v0x7fffd0faa120_0;
    %store/vec4 v0x7fffd0f85660_0, 0, 4;
    %callf/vec4 TD_gray_code.bin_to_gray, S_0x7fffd0f5d6c0;
    %store/vec4 v0x7fffd0faa220_0, 0, 4;
    %delay 10, 0;
    %delay 20, 0;
    %vpi_call 2 24 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "gray_code.v";

S_0x7ffff29232b0 .scope module, "tb_decoder_func" "tb_decoder_func" 2 1;
 .timescale 0 0;
v0x7ffff2937fc0_0 .var "a", 2 0;
v0x7ffff2938080_0 .net "y", 7 0, L_0x7ffff2938150;  1 drivers
S_0x7ffff2923440 .scope task, "apply_input" "apply_input" 2 7, 2 7 0, S_0x7ffff29232b0;
 .timescale 0 0;
v0x7ffff2923620_0 .var "val", 2 0;
TD_tb_decoder_func.apply_input ;
    %load/vec4 v0x7ffff2923620_0;
    %store/vec4 v0x7ffff2937fc0_0, 0, 3;
    %delay 5, 0;
    %vpi_call 2 12 "$display", "T=%0t | a=%b (%0d) ==> y=%b", $time, v0x7ffff2937fc0_0, v0x7ffff2937fc0_0, v0x7ffff2938080_0 {0 0 0};
    %end;
S_0x7ffff29377a0 .scope module, "uut" "decoder_func" 2 5, 3 1 0, S_0x7ffff29232b0;
 .timescale 0 0;
    .port_info 0 /INPUT 3 "a";
    .port_info 1 /OUTPUT 8 "y";
v0x7ffff2937d80_0 .net "a", 2 0, v0x7ffff2937fc0_0;  1 drivers
v0x7ffff2937e80_0 .net "y", 7 0, L_0x7ffff2938150;  alias, 1 drivers
L_0x7ffff2938150 .ufunc/vec4 TD_tb_decoder_func.uut.decode, 8, v0x7ffff2937fc0_0 (v0x7ffff2937ca0_0) S_0x7ffff29379a0;
S_0x7ffff29379a0 .scope function.vec4.s8, "decode" "decode" 3 6, 3 6 0, S_0x7ffff29377a0;
 .timescale 0 0;
; Variable decode is vec4 return value of scope S_0x7ffff29379a0
v0x7ffff2937ca0_0 .var "in", 2 0;
TD_tb_decoder_func.uut.decode ;
    %pushi/vec4 0, 0, 8;
    %ret/vec4 0, 0, 8;  Assign to decode (store_vec4_to_lval)
    %pushi/vec4 1, 0, 1;
    %ix/getv 4, v0x7ffff2937ca0_0;
    %ret/vec4 0, 4, 1; Assign to decode (store_vec4_to_lval)
    %end;
    .scope S_0x7ffff29232b0;
T_2 ;
    %vpi_call 2 17 "$display", "---- Decoder Test ----" {0 0 0};
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x7ffff2923620_0, 0, 3;
    %fork TD_tb_decoder_func.apply_input, S_0x7ffff2923440;
    %join;
    %pushi/vec4 1, 0, 3;
    %store/vec4 v0x7ffff2923620_0, 0, 3;
    %fork TD_tb_decoder_func.apply_input, S_0x7ffff2923440;
    %join;
    %pushi/vec4 2, 0, 3;
    %store/vec4 v0x7ffff2923620_0, 0, 3;
    %fork TD_tb_decoder_func.apply_input, S_0x7ffff2923440;
    %join;
    %pushi/vec4 3, 0, 3;
    %store/vec4 v0x7ffff2923620_0, 0, 3;
    %fork TD_tb_decoder_func.apply_input, S_0x7ffff2923440;
    %join;
    %pushi/vec4 4, 0, 3;
    %store/vec4 v0x7ffff2923620_0, 0, 3;
    %fork TD_tb_decoder_func.apply_input, S_0x7ffff2923440;
    %join;
    %pushi/vec4 5, 0, 3;
    %store/vec4 v0x7ffff2923620_0, 0, 3;
    %fork TD_tb_decoder_func.apply_input, S_0x7ffff2923440;
    %join;
    %pushi/vec4 6, 0, 3;
    %store/vec4 v0x7ffff2923620_0, 0, 3;

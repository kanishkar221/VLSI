#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffd94055a0 .scope module, "tb_adder_blocking" "tb_adder_blocking" 2 1;
 .timescale 0 0;
v0x7fffd942c2f0_0 .var "a", 1 0;
v0x7fffd942c3e0_0 .var "b", 1 0;
v0x7fffd942c4b0_0 .net "sum", 2 0, v0x7fffd942c1b0_0;  1 drivers
S_0x7fffd9418bc0 .scope module, "dut" "adder_blocking" 2 5, 3 1 0, S_0x7fffd94055a0;
 .timescale 0 0;
    .port_info 0 /INPUT 2 "a";
    .port_info 1 /INPUT 2 "b";
    .port_info 2 /OUTPUT 3 "sum";
v0x7fffd9418e10_0 .net "a", 1 0, v0x7fffd942c2f0_0;  1 drivers
v0x7fffd942c0d0_0 .net "b", 1 0, v0x7fffd942c3e0_0;  1 drivers
v0x7fffd942c1b0_0 .var "sum", 2 0;
E_0x7fffd9418460 .event anyedge, v0x7fffd9418e10_0, v0x7fffd942c0d0_0;
    .scope S_0x7fffd9418bc0;
T_0 ;
    %wait E_0x7fffd9418460;
    %load/vec4 v0x7fffd9418e10_0;
    %pad/u 3;
    %load/vec4 v0x7fffd942c0d0_0;
    %pad/u 3;
    %add;
    %store/vec4 v0x7fffd942c1b0_0, 0, 3;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x7fffd94055a0;
T_1 ;
    %vpi_call 2 8 "$dumpfile", "adder_blocking.vcd" {0 0 0};
    %vpi_call 2 9 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x7fffd94055a0 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x7fffd94055a0;
T_2 ;
    %vpi_call 2 13 "$display", "time\011a\011b\011sum" {0 0 0};
    %vpi_call 2 14 "$monitor", "%0t\011%b\011%b\011%b", $time, v0x7fffd942c2f0_0, v0x7fffd942c3e0_0, v0x7fffd942c4b0_0 {0 0 0};
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x7fffd942c2f0_0, 0, 2;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x7fffd942c3e0_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x7fffd942c2f0_0, 0, 2;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x7fffd942c3e0_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x7fffd942c2f0_0, 0, 2;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x7fffd942c3e0_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x7fffd942c2f0_0, 0, 2;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x7fffd942c3e0_0, 0, 2;
    %delay 5, 0;
    %delay 5, 0;
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.

#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffe0066510 .scope module, "tb_random" "tb_random" 2 1;
 .timescale 0 0;
v0x7fffe00666a0_0 .net *"_ivl_0", 4 0, L_0x7fffe00b1cc0;  1 drivers
L_0x7f4a18040018 .functor BUFT 1, C4<0>, C4<0>, C4<0>, C4<0>;
v0x7fffe00b1670_0 .net *"_ivl_3", 0 0, L_0x7f4a18040018;  1 drivers
v0x7fffe00b1750_0 .net *"_ivl_4", 4 0, L_0x7fffe00b1e30;  1 drivers
L_0x7f4a18040060 .functor BUFT 1, C4<0>, C4<0>, C4<0>, C4<0>;
v0x7fffe00b1810_0 .net *"_ivl_7", 0 0, L_0x7f4a18040060;  1 drivers
v0x7fffe00b18f0_0 .var "a", 3 0;
v0x7fffe00b1a20_0 .var "b", 3 0;
v0x7fffe00b1b00_0 .var/i "i", 31 0;
v0x7fffe00b1be0_0 .net "sum", 4 0, L_0x7fffe00b1f80;  1 drivers
L_0x7fffe00b1cc0 .concat [ 4 1 0 0], v0x7fffe00b18f0_0, L_0x7f4a18040018;
L_0x7fffe00b1e30 .concat [ 4 1 0 0], v0x7fffe00b1a20_0, L_0x7f4a18040060;
L_0x7fffe00b1f80 .arith/sum 5, L_0x7fffe00b1cc0, L_0x7fffe00b1e30;
    .scope S_0x7fffe0066510;
T_0 ;
    %vpi_call 2 9 "$display", "Time |   A   |   B   |  SUM" {0 0 0};
    %vpi_call 2 10 "$monitor", "%4t | %4d | %4d | %4d", $time, v0x7fffe00b18f0_0, v0x7fffe00b1a20_0, v0x7fffe00b1be0_0 {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fffe00b1b00_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x7fffe00b1b00_0;
    %cmpi/s 10, 0, 32;
    %jmp/0xz T_0.1, 5;
    %vpi_func 2 13 "$random" 32 {0 0 0};
    %pushi/vec4 16, 0, 32;
    %mod/s;
    %pad/s 4;
    %store/vec4 v0x7fffe00b18f0_0, 0, 4;
    %vpi_func 2 14 "$random" 32 {0 0 0};
    %pushi/vec4 16, 0, 32;
    %mod/s;
    %pad/s 4;
    %store/vec4 v0x7fffe00b1a20_0, 0, 4;
    %delay 5, 0;
    %load/vec4 v0x7fffe00b1b00_0;
    %addi 1, 0, 32;
    %store/vec4 v0x7fffe00b1b00_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %vpi_call 2 17 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "tb_random.v";

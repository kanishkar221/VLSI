#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffe79f1130 .scope module, "tb_bus_decoder" "tb_bus_decoder" 2 1;
 .timescale 0 0;
v0x7fffe7a03d80_0 .var "addr", 1 0;
v0x7fffe7a03e40_0 .net "enable", 3 0, v0x7fffe7a03c40_0;  1 drivers
S_0x7fffe79f12c0 .scope module, "uut" "bus_decoder" 2 5, 3 1 0, S_0x7fffe79f1130;
 .timescale 0 0;
    .port_info 0 /INPUT 2 "addr";
    .port_info 1 /OUTPUT 4 "enable";
v0x7fffe79f14c0_0 .net "addr", 1 0, v0x7fffe7a03d80_0;  1 drivers
v0x7fffe7a03c40_0 .var "enable", 3 0;
E_0x7fffe79b7a70 .event anyedge, v0x7fffe79f14c0_0;
    .scope S_0x7fffe79f12c0;
T_0 ;
    %wait E_0x7fffe79b7a70;
    %load/vec4 v0x7fffe79f14c0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x7fffe7a03c40_0, 0, 4;
    %jmp T_0.5;
T_0.0 ;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x7fffe7a03c40_0, 0, 4;
    %jmp T_0.5;
T_0.1 ;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x7fffe7a03c40_0, 0, 4;
    %jmp T_0.5;
T_0.2 ;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x7fffe7a03c40_0, 0, 4;
    %jmp T_0.5;
T_0.3 ;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x7fffe7a03c40_0, 0, 4;
    %jmp T_0.5;
T_0.5 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x7fffe79f1130;
T_1 ;
    %vpi_call 2 11 "$monitor", "Time=%0t | addr=%b | enable=%b", $time, v0x7fffe7a03d80_0, v0x7fffe7a03e40_0 {0 0 0};
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x7fffe7a03d80_0, 0, 2;
    %delay 10, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x7fffe7a03d80_0, 0, 2;
    %delay 10, 0;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x7fffe7a03d80_0, 0, 2;
    %delay 10, 0;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x7fffe7a03d80_0, 0, 2;
    %delay 10, 0;
    %pushi/vec4 3, 3, 2;
    %store/vec4 v0x7fffe7a03d80_0, 0, 2;
    %delay 10, 0;
    %vpi_call 2 21 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x7fffe79f1130;
T_2 ;
    %vpi_call 2 25 "$dumpfile", "bus_decoder.vcd" {0 0 0};
    %vpi_call 2 26 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x7fffe79f1130 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_bus_decoder.v";
    "bus_decoder.v";

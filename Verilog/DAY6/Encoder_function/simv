#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffd5f39f30 .scope module, "tb_encoder_func" "tb_encoder_func" 2 1;
 .timescale 0 0;
v0x7fffd5f4f7d0_0 .net "a", 2 0, L_0x7fffd5f4f990;  1 drivers
v0x7fffd5f4f8c0_0 .var "y", 7 0;
S_0x7fffd5f3a0c0 .scope task, "apply_input" "apply_input" 2 8, 2 8 0, S_0x7fffd5f39f30;
 .timescale 0 0;
v0x7fffd5f3a2a0_0 .var "val", 7 0;
TD_tb_encoder_func.apply_input ;
    %load/vec4 v0x7fffd5f3a2a0_0;
    %store/vec4 v0x7fffd5f4f8c0_0, 0, 8;
    %delay 5, 0;
    %vpi_call 2 13 "$display", "T=%0t | y=%b ==> a=%b (%0d)", $time, v0x7fffd5f4f8c0_0, v0x7fffd5f4f7d0_0, v0x7fffd5f4f7d0_0 {0 0 0};
    %end;
S_0x7fffd5f4eef0 .scope module, "uut" "encoder_func" 2 6, 3 1 0, S_0x7fffd5f39f30;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "y";
    .port_info 1 /OUTPUT 3 "a";
v0x7fffd5f4f590_0 .net "a", 2 0, L_0x7fffd5f4f990;  alias, 1 drivers
v0x7fffd5f4f690_0 .net "y", 7 0, v0x7fffd5f4f8c0_0;  1 drivers
L_0x7fffd5f4f990 .ufunc/vec4 TD_tb_encoder_func.uut.encode, 3, v0x7fffd5f4f8c0_0 (v0x7fffd5f4f4d0_0) S_0x7fffd5f4f0f0;
S_0x7fffd5f4f0f0 .scope function.vec4.s3, "encode" "encode" 3 6, 3 6 0, S_0x7fffd5f4eef0;
 .timescale 0 0;
; Variable encode is vec4 return value of scope S_0x7fffd5f4f0f0
v0x7fffd5f4f3f0_0 .var/i "i", 31 0;
v0x7fffd5f4f4d0_0 .var "in", 7 0;
TD_tb_encoder_func.uut.encode ;
    %pushi/vec4 0, 0, 3;
    %ret/vec4 0, 0, 3;  Assign to encode (store_vec4_to_lval)
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fffd5f4f3f0_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x7fffd5f4f3f0_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_1.1, 5;
    %load/vec4 v0x7fffd5f4f4d0_0;
    %load/vec4 v0x7fffd5f4f3f0_0;
    %part/s 1;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %load/vec4 v0x7fffd5f4f3f0_0;
    %parti/s 3, 0, 2;
    %ret/vec4 0, 0, 3;  Assign to encode (store_vec4_to_lval)
T_1.2 ;
    %load/vec4 v0x7fffd5f4f3f0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x7fffd5f4f3f0_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %end;
    .scope S_0x7fffd5f39f30;
T_2 ;
    %vpi_call 2 18 "$display", "---- Encoder Test ----" {0 0 0};
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x7fffd5f3a2a0_0, 0, 8;
    %fork TD_tb_encoder_func.apply_input, S_0x7fffd5f3a0c0;
    %join;
    %pushi/vec4 2, 0, 8;
    %store/vec4 v0x7fffd5f3a2a0_0, 0, 8;
    %fork TD_tb_encoder_func.apply_input, S_0x7fffd5f3a0c0;
    %join;
    %pushi/vec4 4, 0, 8;
    %store/vec4 v0x7fffd5f3a2a0_0, 0, 8;
    %fork TD_tb_encoder_func.apply_input, S_0x7fffd5f3a0c0;
    %join;
    %pushi/vec4 8, 0, 8;
    %store/vec4 v0x7fffd5f3a2a0_0, 0, 8;
    %fork TD_tb_encoder_func.apply_input, S_0x7fffd5f3a0c0;
    %join;
    %pushi/vec4 16, 0, 8;
    %store/vec4 v0x7fffd5f3a2a0_0, 0, 8;
    %fork TD_tb_encoder_func.apply_input, S_0x7fffd5f3a0c0;
    %join;
    %pushi/vec4 32, 0, 8;
    %store/vec4 v0x7fffd5f3a2a0_0, 0, 8;
    %fork TD_tb_encoder_func.apply_input, S_0x7fffd5f3a0c0;
    %join;
    %pushi/vec4 64, 0, 8;
    %store/vec4 v0x7fffd5f3a2a0_0, 0, 8;
    %fork TD_tb_encoder_func.apply_input, S_0x7fffd5f3a0c0;
    %join;
    %pushi/vec4 128, 0, 8;
    %store/vec4 v0x7fffd5f3a2a0_0, 0, 8;
    %fork TD_tb_encoder_func.apply_input, S_0x7fffd5f3a0c0;
    %join;
    %delay 10, 0;
    %vpi_call 2 27 "$finish" {0 0 0};
    %end;
    .thread T_2;

    %store/vec4 v0x7fffd1c00780_0, 0, 8;
    %load/vec4 v0x7fffd1c00680_0;
    %store/vec4 v0x7fffd1c004e0_0, 0, 8;
    %load/vec4 v0x7fffd1c00780_0;
    %store/vec4 v0x7fffd1c005c0_0, 0, 8;
    %fork TD_signed_add.add_task, S_0x7fffd1bb36d0;
    %join;

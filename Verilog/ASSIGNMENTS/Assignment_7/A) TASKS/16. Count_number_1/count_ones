S_0x7fffc7ce8590 .scope module, "count_ones" "count_ones" 2 1;
 .timescale 0 0;
v0x7fffc7d35880_0 .var "data", 7 0;
v0x7fffc7d35980_0 .var/i "ones_count", 31 0;
S_0x7fffc7ce8720 .scope task, "count_ones_task" "count_ones_task" 2 5, 2 5 0, S_0x7fffc7ce8590;
 .timescale 0 0;
v0x7fffc7d10cf0_0 .var/i "count", 31 0;
v0x7fffc7d356e0_0 .var/i "i", 31 0;
v0x7fffc7d357c0_0 .var "in_val", 7 0;
TD_count_ones.count_ones_task ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fffc7d10cf0_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fffc7d356e0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x7fffc7d356e0_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x7fffc7d357c0_0;
    %load/vec4 v0x7fffc7d356e0_0;
    %part/s 1;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x7fffc7d10cf0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x7fffc7d10cf0_0, 0, 32;
T_0.2 ;
    %load/vec4 v0x7fffc7d356e0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x7fffc7d356e0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .scope S_0x7fffc7ce8590;
T_1 ;
    %vpi_call 2 19 "$monitor", "Time=%0t | data=%b | ones_count=%0d", $time, v0x7fffc7d35880_0, v0x7fffc7d35980_0 {0 0 0};
    %pushi/vec4 170, 0, 8;
    %store/vec4 v0x7fffc7d35880_0, 0, 8;
    %load/vec4 v0x7fffc7d35880_0;
    %store/vec4 v0x7fffc7d357c0_0, 0, 8;
    %fork TD_count_ones.count_ones_task, S_0x7fffc7ce8720;
    %join;
    %load/vec4 v0x7fffc7d10cf0_0;
    %store/vec4 v0x7fffc7d35980_0, 0, 32;
    %delay 5, 0;
    %pushi/vec4 240, 0, 8;
    %store/vec4 v0x7fffc7d35880_0, 0, 8;
    %load/vec4 v0x7fffc7d35880_0;
    %store/vec4 v0x7fffc7d357c0_0, 0, 8;

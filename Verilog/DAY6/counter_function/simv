#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffefc94a10 .scope module, "counter_func_tb" "counter_func_tb" 2 1;
 .timescale 0 0;
v0x7fffefcad490_0 .var "clk", 0 0;
v0x7fffefcad550_0 .var "dir", 0 0;
v0x7fffefcad620_0 .var "en", 0 0;
v0x7fffefcad720_0 .net "q", 3 0, v0x7fffefcad200_0;  1 drivers
v0x7fffefcad7f0_0 .var "rst_n", 0 0;
S_0x7fffefc94ba0 .scope task, "do_count" "do_count" 2 17, 2 17 0, S_0x7fffefc94a10;
 .timescale 0 0;
v0x7fffefc843d0_0 .var "dir_in", 0 0;
v0x7fffefcac530_0 .var/i "n", 31 0;
E_0x7fffefc92510 .event posedge, v0x7fffefcacf90_0;
TD_counter_func_tb.do_count ;
    %load/vec4 v0x7fffefc843d0_0;
    %store/vec4 v0x7fffefcad550_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffefcad620_0, 0, 1;
    %load/vec4 v0x7fffefcac530_0;
T_0.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_0.1, 5;
    %jmp/1 T_0.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x7fffefc92510;
    %jmp T_0.0;
T_0.1 ;
    %pop/vec4 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffefcad620_0, 0, 1;
    %end;
S_0x7fffefcac610 .scope task, "reset_counter" "reset_counter" 2 10, 2 10 0, S_0x7fffefc94a10;
 .timescale 0 0;
TD_counter_func_tb.reset_counter ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffefcad7f0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffefcad620_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffefcad550_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffefcad7f0_0, 0, 1;
    %end;
S_0x7fffefcac810 .scope module, "uut" "counter_func" 2 5, 3 1 0, S_0x7fffefc94a10;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst_n";
    .port_info 2 /INPUT 1 "en";
    .port_info 3 /INPUT 1 "dir";
    .port_info 4 /OUTPUT 4 "q";
v0x7fffefcacf90_0 .net "clk", 0 0, v0x7fffefcad490_0;  1 drivers
v0x7fffefcad070_0 .net "dir", 0 0, v0x7fffefcad550_0;  1 drivers
v0x7fffefcad130_0 .net "en", 0 0, v0x7fffefcad620_0;  1 drivers
v0x7fffefcad200_0 .var "q", 3 0;
v0x7fffefcad2e0_0 .net "rst_n", 0 0, v0x7fffefcad7f0_0;  1 drivers
E_0x7fffefc59b40/0 .event negedge, v0x7fffefcad2e0_0;
E_0x7fffefc59b40/1 .event posedge, v0x7fffefcacf90_0;
E_0x7fffefc59b40 .event/or E_0x7fffefc59b40/0, E_0x7fffefc59b40/1;
S_0x7fffefcaca40 .scope function.vec4.s4, "next_count" "next_count" 3 9, 3 9 0, S_0x7fffefcac810;
 .timescale 0 0;
v0x7fffefcacc20_0 .var "curr", 3 0;
v0x7fffefcacd20_0 .var "dir_in", 0 0;
v0x7fffefcacde0_0 .var "en_in", 0 0;
; Variable next_count is vec4 return value of scope S_0x7fffefcaca40
TD_counter_func_tb.uut.next_count ;
    %load/vec4 v0x7fffefcacde0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.2, 8;
    %load/vec4 v0x7fffefcacc20_0;
    %ret/vec4 0, 0, 4;  Assign to next_count (store_vec4_to_lval)
    %jmp T_2.3;
T_2.2 ;
    %load/vec4 v0x7fffefcacd20_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.4, 8;
    %load/vec4 v0x7fffefcacc20_0;
    %addi 1, 0, 4;
    %ret/vec4 0, 0, 4;  Assign to next_count (store_vec4_to_lval)
    %jmp T_2.5;
T_2.4 ;
    %load/vec4 v0x7fffefcacc20_0;
    %subi 1, 0, 4;
    %ret/vec4 0, 0, 4;  Assign to next_count (store_vec4_to_lval)
T_2.5 ;
T_2.3 ;
    %end;
    .scope S_0x7fffefcac810;
T_3 ;
    %wait E_0x7fffefc59b40;
    %load/vec4 v0x7fffefcad2e0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x7fffefcad200_0, 0;
    %jmp T_3.1;
T_3.0 ;
    %load/vec4 v0x7fffefcad200_0;
    %load/vec4 v0x7fffefcad070_0;
    %load/vec4 v0x7fffefcad130_0;
    %store/vec4 v0x7fffefcacde0_0, 0, 1;
    %store/vec4 v0x7fffefcacd20_0, 0, 1;
    %store/vec4 v0x7fffefcacc20_0, 0, 4;
    %callf/vec4 TD_counter_func_tb.uut.next_count, S_0x7fffefcaca40;
    %assign/vec4 v0x7fffefcad200_0, 0;
T_3.1 ;
    %jmp T_3;
    .thread T_3;
    .scope S_0x7fffefc94a10;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffefcad490_0, 0, 1;
    %end;
    .thread T_4;
    .scope S_0x7fffefc94a10;
T_5 ;
    %delay 5, 0;
    %load/vec4 v0x7fffefcad490_0;
    %inv;
    %store/vec4 v0x7fffefcad490_0, 0, 1;
    %jmp T_5;
    .thread T_5;
    .scope S_0x7fffefc94a10;
T_6 ;
    %vpi_call 2 28 "$display", "---- Simple Counter Test ----" {0 0 0};
    %fork TD_counter_func_tb.reset_counter, S_0x7fffefcac610;
    %join;
    %pushi/vec4 5, 0, 32;
    %store/vec4 v0x7fffefcac530_0, 0, 32;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffefc843d0_0, 0, 1;
    %fork TD_counter_func_tb.do_count, S_0x7fffefc94ba0;
    %join;
    %pushi/vec4 3, 0, 32;
    %store/vec4 v0x7fffefcac530_0, 0, 32;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffefc843d0_0, 0, 1;
    %fork TD_counter_func_tb.do_count, S_0x7fffefc94ba0;
    %join;
    %delay 20, 0;
    %vpi_call 2 32 "$finish" {0 0 0};
    %end;
    .thread T_6;
    .scope S_0x7fffefc94a10;
T_7 ;
    %wait E_0x7fffefc92510;
    %vpi_call 2 36 "$display", "T=%0t | q=%0d", $time, v0x7fffefcad720_0 {0 0 0};
    %jmp T_7;
    .thread T_7;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "counter_func_tb.v";
    "counter_func.v";

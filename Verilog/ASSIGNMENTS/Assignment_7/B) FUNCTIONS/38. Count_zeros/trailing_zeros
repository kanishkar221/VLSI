#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffd4bdc590 .scope module, "trailing_zeros" "trailing_zeros" 2 1;
 .timescale 0 0;
v0x7fffd4c2ac90_0 .var/i "count", 31 0;
v0x7fffd4c2ad90_0 .var "num", 7 0;
S_0x7fffd4bdc720 .scope function.vec4.u32, "count_tz" "count_tz" 2 6, 2 6 0, S_0x7fffd4bdc590;
 .timescale 0 0;
; Variable count_tz is vec4 return value of scope S_0x7fffd4bdc720
v0x7fffd4c2aaf0_0 .var/i "i", 31 0;
v0x7fffd4c2abd0_0 .var "x", 7 0;
TD_trailing_zeros.count_tz ;
    %pushi/vec4 0, 0, 32;
    %ret/vec4 0, 0, 32;  Assign to count_tz (store_vec4_to_lval)
    %load/vec4 v0x7fffd4c2abd0_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 8, 0, 32;
    %ret/vec4 0, 0, 32;  Assign to count_tz (store_vec4_to_lval)
    %jmp T_0.1;
T_0.0 ;
    %fork t_1, S_0x7fffd4c05360;
    %jmp t_0;
    .scope S_0x7fffd4c05360;
t_1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fffd4c2aaf0_0, 0, 32;
T_0.2 ;
    %load/vec4 v0x7fffd4c2aaf0_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.3, 5;
    %load/vec4 v0x7fffd4c2abd0_0;
    %load/vec4 v0x7fffd4c2aaf0_0;
    %part/s 1;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_0.4, 4;
    %retload/vec4 0; Load count_tz (draw_signal_vec4)
    %addi 1, 0, 32;
    %ret/vec4 0, 0, 32;  Assign to count_tz (store_vec4_to_lval)
    %jmp T_0.5;
T_0.4 ;
    %disable S_0x7fffd4c05360;
T_0.5 ;
    %load/vec4 v0x7fffd4c2aaf0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x7fffd4c2aaf0_0, 0, 32;
    %jmp T_0.2;
T_0.3 ;
    %end;
    .scope S_0x7fffd4bdc720;
t_0 %join;
T_0.1 ;
    %end;
S_0x7fffd4c05360 .scope begin, "loop_block" "loop_block" 2 13, 2 13 0, S_0x7fffd4bdc720;
 .timescale 0 0;
    .scope S_0x7fffd4bdc590;
T_1 ;
    %vpi_call 2 25 "$monitor", "Time=%0t | num=%b (%0d) | trailing_zeros=%0d", $time, v0x7fffd4c2ad90_0, v0x7fffd4c2ad90_0, v0x7fffd4c2ac90_0 {0 0 0};
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x7fffd4c2ad90_0, 0, 8;
    %load/vec4 v0x7fffd4c2ad90_0;
    %store/vec4 v0x7fffd4c2abd0_0, 0, 8;
    %callf/vec4 TD_trailing_zeros.count_tz, S_0x7fffd4bdc720;
    %store/vec4 v0x7fffd4c2ac90_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x7fffd4c2ad90_0, 0, 8;
    %load/vec4 v0x7fffd4c2ad90_0;
    %store/vec4 v0x7fffd4c2abd0_0, 0, 8;
    %callf/vec4 TD_trailing_zeros.count_tz, S_0x7fffd4bdc720;
    %store/vec4 v0x7fffd4c2ac90_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 20, 0, 8;
    %store/vec4 v0x7fffd4c2ad90_0, 0, 8;
    %load/vec4 v0x7fffd4c2ad90_0;
    %store/vec4 v0x7fffd4c2abd0_0, 0, 8;
    %callf/vec4 TD_trailing_zeros.count_tz, S_0x7fffd4bdc720;
    %store/vec4 v0x7fffd4c2ac90_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 128, 0, 8;
    %store/vec4 v0x7fffd4c2ad90_0, 0, 8;
    %load/vec4 v0x7fffd4c2ad90_0;
    %store/vec4 v0x7fffd4c2abd0_0, 0, 8;
    %callf/vec4 TD_trailing_zeros.count_tz, S_0x7fffd4bdc720;
    %store/vec4 v0x7fffd4c2ac90_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 254, 0, 8;
    %store/vec4 v0x7fffd4c2ad90_0, 0, 8;
    %load/vec4 v0x7fffd4c2ad90_0;
    %store/vec4 v0x7fffd4c2abd0_0, 0, 8;
    %callf/vec4 TD_trailing_zeros.count_tz, S_0x7fffd4bdc720;
    %store/vec4 v0x7fffd4c2ac90_0, 0, 32;
    %delay 10, 0;
    %delay 20, 0;
    %vpi_call 2 33 "$finish" {0 0 0};
    %end;
    .thread T_1;

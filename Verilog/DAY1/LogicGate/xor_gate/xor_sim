S_0x7ffff4482890 .scope module, "tb_xor" "tb_xor" 2 1;
 .timescale 0 0;
v0x7ffff44a8c10_0 .var "a", 0 0;
v0x7ffff44a8ce0_0 .var "b", 0 0;
v0x7ffff44a8db0_0 .net "o", 0 0, L_0x7ffff44a8eb0;  1 drivers
S_0x7ffff445d4f0 .scope module, "uut" "xor_gate" 2 4, 3 1 0, S_0x7ffff4482890;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "o";
L_0x7ffff44a8eb0 .functor XOR 1, v0x7ffff44a8c10_0, v0x7ffff44a8ce0_0, C4<0>, C4<0>;
v0x7ffff445d720_0 .net "a", 0 0, v0x7ffff44a8c10_0;  1 drivers
v0x7ffff44a8a30_0 .net "b", 0 0, v0x7ffff44a8ce0_0;  1 drivers
v0x7ffff44a8af0_0 .net "o", 0 0, L_0x7ffff44a8eb0;  alias, 1 drivers
    .scope S_0x7ffff4482890;
T_0 ;
    %vpi_call 2 7 "$dumpfile", "xor_wave.vcd" {0 0 0};
    %vpi_call 2 8 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x7ffff4482890 {0 0 0};
    %vpi_call 2 10 "$display", "time | a b | o" {0 0 0};
    %vpi_call 2 11 "$monitor", "%4t | %b %b | %b", $time, v0x7ffff44a8c10_0, v0x7ffff44a8ce0_0, v0x7ffff44a8db0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7ffff44a8c10_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7ffff44a8ce0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7ffff44a8c10_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7ffff44a8ce0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7ffff44a8c10_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7ffff44a8ce0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7ffff44a8c10_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7ffff44a8ce0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 17 "$finish" {0 0 0};
    %end;
    .thread T_0;

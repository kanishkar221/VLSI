#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffb78ce4c0 .scope module, "tb_wrong_casex" "tb_wrong_casex" 2 1;
 .timescale 0 0;
v0x7fffb791a830_0 .var "in", 3 0;
v0x7fffb791a8f0_0 .net "y", 0 0, v0x7fffb791a710_0;  1 drivers
S_0x7fffb78ce650 .scope module, "uut" "wrong_casex" 2 6, 3 1 0, S_0x7fffb78ce4c0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "in";
    .port_info 1 /OUTPUT 1 "y";
v0x7fffb78ce800_0 .net "in", 3 0, v0x7fffb791a830_0;  1 drivers
v0x7fffb791a710_0 .var "y", 0 0;
E_0x7fffb7908670 .event anyedge, v0x7fffb78ce800_0;
    .scope S_0x7fffb78ce650;
T_0 ;
    %wait E_0x7fffb7908670;
    %load/vec4 v0x7fffb78ce800_0;
    %dup/vec4;
    %pushi/vec4 12, 4, 4;
    %cmp/x;
    %jmp/1 T_0.0, 4;
    %dup/vec4;
    %pushi/vec4 4, 4, 4;
    %cmp/x;
    %jmp/1 T_0.1, 4;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffb791a710_0, 0, 1;
    %jmp T_0.3;
T_0.0 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffb791a710_0, 0, 1;
    %jmp T_0.3;
T_0.1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffb791a710_0, 0, 1;
    %jmp T_0.3;
T_0.3 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x7fffb78ce4c0;
T_1 ;
    %vpi_call 2 9 "$dumpfile", "wrong_casex.vcd" {0 0 0};
    %vpi_call 2 10 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x7fffb78ce4c0 {0 0 0};
    %vpi_call 2 11 "$monitor", "Time=%0t | in=%b | y=%b", $time, v0x7fffb791a830_0, v0x7fffb791a8f0_0 {0 0 0};
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x7fffb791a830_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 12, 0, 4;
    %store/vec4 v0x7fffb791a830_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x7fffb791a830_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x7fffb791a830_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x7fffb791a830_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 12, 4, 4;
    %store/vec4 v0x7fffb791a830_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 4, 4, 4;
    %store/vec4 v0x7fffb791a830_0, 0, 4;
    %delay 10, 0;
    %delay 50, 0;
    %vpi_call 2 23 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;

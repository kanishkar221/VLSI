#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffefd6f570 .scope module, "factorial" "factorial" 2 1;
 .timescale 0 0;
v0x7fffefdbc930_0 .var "fact", 31 0;
v0x7fffefdbca30_0 .var "num", 7 0;
S_0x7fffefd6f700 .scope task, "calc_factorial" "calc_factorial" 2 6, 2 6 0, S_0x7fffefd6f570;
 .timescale 0 0;
v0x7fffefd97c80_0 .var "f", 31 0;
v0x7fffefdbc790_0 .var/i "j", 31 0;
v0x7fffefdbc870_0 .var "n", 7 0;
TD_factorial.calc_factorial ;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x7fffefd97c80_0, 0, 32;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x7fffefdbc790_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x7fffefdbc790_0;
    %load/vec4 v0x7fffefdbc870_0;
    %pad/u 32;
    %cmp/u;
    %flag_or 5, 4;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x7fffefd97c80_0;
    %load/vec4 v0x7fffefdbc790_0;
    %mul;
    %store/vec4 v0x7fffefd97c80_0, 0, 32;
    %load/vec4 v0x7fffefdbc790_0;
    %addi 1, 0, 32;
    %store/vec4 v0x7fffefdbc790_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .scope S_0x7fffefd6f570;
T_1 ;
    %vpi_call 2 19 "$monitor", "Time=%0t | num=%0d | factorial=%0d", $time, v0x7fffefdbca30_0, v0x7fffefdbc930_0 {0 0 0};
    %pushi/vec4 5, 0, 8;
    %store/vec4 v0x7fffefdbca30_0, 0, 8;
    %load/vec4 v0x7fffefdbca30_0;
    %store/vec4 v0x7fffefdbc870_0, 0, 8;
    %fork TD_factorial.calc_factorial, S_0x7fffefd6f700;
    %join;
    %load/vec4 v0x7fffefd97c80_0;
    %store/vec4 v0x7fffefdbc930_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 6, 0, 8;
    %store/vec4 v0x7fffefdbca30_0, 0, 8;
    %load/vec4 v0x7fffefdbca30_0;
    %store/vec4 v0x7fffefdbc870_0, 0, 8;
    %fork TD_factorial.calc_factorial, S_0x7fffefd6f700;
    %join;
    %load/vec4 v0x7fffefd97c80_0;
    %store/vec4 v0x7fffefdbc930_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 7, 0, 8;
    %store/vec4 v0x7fffefdbca30_0, 0, 8;
    %load/vec4 v0x7fffefdbca30_0;
    %store/vec4 v0x7fffefdbc870_0, 0, 8;
    %fork TD_factorial.calc_factorial, S_0x7fffefd6f700;
    %join;
    %load/vec4 v0x7fffefd97c80_0;
    %store/vec4 v0x7fffefdbc930_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 8, 0, 8;
    %store/vec4 v0x7fffefdbca30_0, 0, 8;
    %load/vec4 v0x7fffefdbca30_0;
    %store/vec4 v0x7fffefdbc870_0, 0, 8;
    %fork TD_factorial.calc_factorial, S_0x7fffefd6f700;
    %join;
    %load/vec4 v0x7fffefd97c80_0;
    %store/vec4 v0x7fffefdbc930_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 10, 0, 8;
    %store/vec4 v0x7fffefdbca30_0, 0, 8;
    %load/vec4 v0x7fffefdbca30_0;
    %store/vec4 v0x7fffefdbc870_0, 0, 8;
    %fork TD_factorial.calc_factorial, S_0x7fffefd6f700;
    %join;
    %load/vec4 v0x7fffefd97c80_0;
    %store/vec4 v0x7fffefdbc930_0, 0, 32;
    %delay 10, 0;
    %delay 50, 0;
    %vpi_call 2 27 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";

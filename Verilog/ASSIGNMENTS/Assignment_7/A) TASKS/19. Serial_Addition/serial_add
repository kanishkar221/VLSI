v0x7fffdf13c7a0_0 .var "x", 3 0;
v0x7fffdf13c8d0_0 .var "y", 3 0;
TD_serial_add.serial_add_task ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffdf0ec800_0, 0, 1;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0x7fffdf13c6c0_0, 0, 5;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fffdf13c520_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x7fffdf13c520_0;
    %cmpi/s 4, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x7fffdf13c7a0_0;
    %load/vec4 v0x7fffdf13c520_0;
    %part/s 1;
    %load/vec4 v0x7fffdf13c8d0_0;
    %load/vec4 v0x7fffdf13c520_0;
    %part/s 1;
    %xor;
    %load/vec4 v0x7fffdf0ec800_0;
    %xor;
    %ix/getv/s 4, v0x7fffdf13c520_0;
    %store/vec4 v0x7fffdf13c6c0_0, 4, 1;
    %load/vec4 v0x7fffdf13c7a0_0;
    %load/vec4 v0x7fffdf13c520_0;
    %part/s 1;
    %load/vec4 v0x7fffdf13c8d0_0;
    %load/vec4 v0x7fffdf13c520_0;
    %part/s 1;
    %and;
    %load/vec4 v0x7fffdf13c7a0_0;
    %load/vec4 v0x7fffdf13c520_0;
    %part/s 1;
    %load/vec4 v0x7fffdf0ec800_0;
    %and;
    %or;
    %load/vec4 v0x7fffdf13c8d0_0;
    %load/vec4 v0x7fffdf13c520_0;
    %part/s 1;
    %load/vec4 v0x7fffdf0ec800_0;
    %and;
    %or;
    %store/vec4 v0x7fffdf0ec800_0, 0, 1;
    %delay 5, 0;
    %load/vec4 v0x7fffdf13c520_0;
    %addi 1, 0, 32;
    %store/vec4 v0x7fffdf13c520_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %load/vec4 v0x7fffdf0ec800_0;
    %ix/load 4, 4, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x7fffdf13c6c0_0, 4, 1;
    %load/vec4 v0x7fffdf13c6c0_0;
    %store/vec4 v0x7fffdf13c600_0, 0, 5;
    %end;
    .scope S_0x7fffdf0ec490;
T_1 ;
    %vpi_call 2 26 "$monitor", "Time=%0t | a=%b | b=%b | sum=%b (decimal=%0d)", $time, v0x7fffdf13c9b0_0, v0x7fffdf13cab0_0, v0x7fffdf13cb90_0, v0x7fffdf13cb90_0 {0 0 0};
    %pushi/vec4 11, 0, 4;
    %store/vec4 v0x7fffdf13c9b0_0, 0, 4;
    %pushi/vec4 6, 0, 4;
    %store/vec4 v0x7fffdf13cab0_0, 0, 4;
    %load/vec4 v0x7fffdf13c9b0_0;
    %store/vec4 v0x7fffdf13c7a0_0, 0, 4;
    %load/vec4 v0x7fffdf13cab0_0;
    %store/vec4 v0x7fffdf13c8d0_0, 0, 4;
    %fork TD_serial_add.serial_add_task, S_0x7fffdf0ec620;
    %join;
    %load/vec4 v0x7fffdf13c600_0;
    %store/vec4 v0x7fffdf13cb90_0, 0, 5;
    %delay 30, 0;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x7fffdf13c9b0_0, 0, 4;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x7fffdf13cab0_0, 0, 4;

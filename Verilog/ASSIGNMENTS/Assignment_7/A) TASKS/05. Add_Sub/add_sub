S_0x7fffc5bc14f0 .scope module, "add_sub_task" "add_sub_task" 2 1;
 .timescale 0 0;
v0x7fffc5c10c00_0 .var "a", 7 0;
v0x7fffc5c10d00_0 .var "b", 7 0;
v0x7fffc5c10de0_0 .var "diff", 8 0;
v0x7fffc5c10ed0_0 .var "sum", 8 0;
S_0x7fffc5bc1680 .scope task, "add_sub_task" "add_sub_task" 2 6, 2 6 0, S_0x7fffc5bc14f0;
 .timescale 0 0;
v0x7fffc5bc1860_0 .var "add_result", 8 0;
v0x7fffc5c10980_0 .var "sub_result", 8 0;
v0x7fffc5c10a60_0 .var "x", 7 0;
v0x7fffc5c10b20_0 .var "y", 7 0;
TD_add_sub_task.add_sub_task ;
    %load/vec4 v0x7fffc5c10a60_0;
    %pad/u 9;
    %load/vec4 v0x7fffc5c10b20_0;
    %pad/u 9;
    %add;
    %store/vec4 v0x7fffc5bc1860_0, 0, 9;
    %pushi/vec4 0, 0, 1;
    %load/vec4 v0x7fffc5c10a60_0;
    %concat/vec4; draw_concat_vec4
    %pushi/vec4 0, 0, 1;
    %load/vec4 v0x7fffc5c10b20_0;
    %concat/vec4; draw_concat_vec4
    %sub;
    %store/vec4 v0x7fffc5c10980_0, 0, 9;
    %end;
    .scope S_0x7fffc5bc14f0;
T_1 ;
    %vpi_call 2 17 "$monitor", "Time=%0t | a=%d | b=%d | sum=%d | diff=%d", $time, v0x7fffc5c10c00_0, v0x7fffc5c10d00_0, v0x7fffc5c10ed0_0, v0x7fffc5c10de0_0 {0 0 0};
    %pushi/vec4 15, 0, 8;
    %store/vec4 v0x7fffc5c10c00_0, 0, 8;
    %pushi/vec4 10, 0, 8;
    %store/vec4 v0x7fffc5c10d00_0, 0, 8;
    %load/vec4 v0x7fffc5c10c00_0;
    %store/vec4 v0x7fffc5c10a60_0, 0, 8;
    %load/vec4 v0x7fffc5c10d00_0;
    %store/vec4 v0x7fffc5c10b20_0, 0, 8;
    %fork TD_add_sub_task.add_sub_task, S_0x7fffc5bc1680;
    %join;
    %load/vec4 v0x7fffc5bc1860_0;
    %store/vec4 v0x7fffc5c10ed0_0, 0, 9;
    %load/vec4 v0x7fffc5c10980_0;
    %store/vec4 v0x7fffc5c10de0_0, 0, 9;
    %delay 10, 0;
    %pushi/vec4 50, 0, 8;
    %store/vec4 v0x7fffc5c10c00_0, 0, 8;

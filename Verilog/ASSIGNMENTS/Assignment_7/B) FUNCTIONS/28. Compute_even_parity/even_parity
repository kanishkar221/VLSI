#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffe0c91590 .scope module, "even_parity" "even_parity" 2 1;
 .timescale 0 0;
v0x7fffe0cde8d0_0 .var "data", 7 0;
v0x7fffe0cde9d0_0 .var "parity", 0 0;
S_0x7fffe0c91720 .scope function.vec4.s1, "parity_even" "parity_even" 2 6, 2 6 0, S_0x7fffe0c91590;
 .timescale 0 0;
v0x7fffe0cb9d50_0 .var/i "i", 31 0;
v0x7fffe0cde690_0 .var "p", 0 0;
; Variable parity_even is vec4 return value of scope S_0x7fffe0c91720
v0x7fffe0cde7f0_0 .var "x", 7 0;
TD_even_parity.parity_even ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffe0cde690_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fffe0cb9d50_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x7fffe0cb9d50_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x7fffe0cde690_0;
    %load/vec4 v0x7fffe0cde7f0_0;
    %load/vec4 v0x7fffe0cb9d50_0;
    %part/s 1;
    %xor;
    %store/vec4 v0x7fffe0cde690_0, 0, 1;
    %load/vec4 v0x7fffe0cb9d50_0;
    %addi 1, 0, 32;
    %store/vec4 v0x7fffe0cb9d50_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %load/vec4 v0x7fffe0cde690_0;
    %inv;
    %ret/vec4 0, 0, 1;  Assign to parity_even (store_vec4_to_lval)
    %end;
    .scope S_0x7fffe0c91590;
T_1 ;
    %vpi_call 2 20 "$monitor", "Time=%0t | data=%b | parity=%b", $time, v0x7fffe0cde8d0_0, v0x7fffe0cde9d0_0 {0 0 0};
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x7fffe0cde8d0_0, 0, 8;
    %load/vec4 v0x7fffe0cde8d0_0;
    %store/vec4 v0x7fffe0cde7f0_0, 0, 8;
    %callf/vec4 TD_even_parity.parity_even, S_0x7fffe0c91720;
    %store/vec4 v0x7fffe0cde9d0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x7fffe0cde8d0_0, 0, 8;
    %load/vec4 v0x7fffe0cde8d0_0;
    %store/vec4 v0x7fffe0cde7f0_0, 0, 8;
    %callf/vec4 TD_even_parity.parity_even, S_0x7fffe0c91720;
    %store/vec4 v0x7fffe0cde9d0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 170, 0, 8;
    %store/vec4 v0x7fffe0cde8d0_0, 0, 8;
    %load/vec4 v0x7fffe0cde8d0_0;
    %store/vec4 v0x7fffe0cde7f0_0, 0, 8;
    %callf/vec4 TD_even_parity.parity_even, S_0x7fffe0c91720;
    %store/vec4 v0x7fffe0cde9d0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 240, 0, 8;
    %store/vec4 v0x7fffe0cde8d0_0, 0, 8;
    %load/vec4 v0x7fffe0cde8d0_0;
    %store/vec4 v0x7fffe0cde7f0_0, 0, 8;
    %callf/vec4 TD_even_parity.parity_even, S_0x7fffe0c91720;
    %store/vec4 v0x7fffe0cde9d0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0x7fffe0cde8d0_0, 0, 8;
    %load/vec4 v0x7fffe0cde8d0_0;
    %store/vec4 v0x7fffe0cde7f0_0, 0, 8;
    %callf/vec4 TD_even_parity.parity_even, S_0x7fffe0c91720;
    %store/vec4 v0x7fffe0cde9d0_0, 0, 1;
    %delay 10, 0;
    %delay 20, 0;
    %vpi_call 2 28 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;

:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffe4d66250 .scope module, "tb_case_compare" "tb_case_compare" 2 1;
 .timescale 0 0;
v0x7fffe4d7f540_0 .var "sel", 1 0;
v0x7fffe4d7f600_0 .net "y_case", 0 0, v0x7fffe4d53760_0;  1 drivers
v0x7fffe4d7f6c0_0 .net "y_casex", 0 0, v0x7fffe4d7f030_0;  1 drivers
v0x7fffe4d7f7c0_0 .net "y_casez", 0 0, v0x7fffe4d7f420_0;  1 drivers
S_0x7fffe4d663e0 .scope module, "u1" "case_example" 2 5, 3 1 0, S_0x7fffe4d66250;
 .timescale 0 0;
    .port_info 0 /INPUT 2 "sel";
    .port_info 1 /OUTPUT 1 "y";
v0x7fffe4d53fb0_0 .net "sel", 1 0, v0x7fffe4d7f540_0;  1 drivers
v0x7fffe4d53760_0 .var "y", 0 0;
E_0x7fffe4d2ca70 .event anyedge, v0x7fffe4d53fb0_0;
S_0x7fffe4d7ed70 .scope module, "u2" "casex_example" 2 6, 3 16 0, S_0x7fffe4d66250;
 .timescale 0 0;
    .port_info 0 /INPUT 2 "sel";
    .port_info 1 /OUTPUT 1 "y";
v0x7fffe4d7ef50_0 .net "sel", 1 0, v0x7fffe4d7f540_0;  alias, 1 drivers
v0x7fffe4d7f030_0 .var "y", 0 0;
S_0x7fffe4d7f130 .scope module, "u3" "casez_example" 2 7, 3 31 0, S_0x7fffe4d66250;
 .timescale 0 0;
    .port_info 0 /INPUT 2 "sel";
    .port_info 1 /OUTPUT 1 "y";
v0x7fffe4d7f310_0 .net "sel", 1 0, v0x7fffe4d7f540_0;  alias, 1 drivers
v0x7fffe4d7f420_0 .var "y", 0 0;
    .scope S_0x7fffe4d663e0;
T_0 ;
    %wait E_0x7fffe4d2ca70;
    %load/vec4 v0x7fffe4d53fb0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %pushi/vec4 1, 1, 1;
    %store/vec4 v0x7fffe4d53760_0, 0, 1;
    %jmp T_0.5;
T_0.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffe4d53760_0, 0, 1;
    %jmp T_0.5;
T_0.1 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffe4d53760_0, 0, 1;
    %jmp T_0.5;
T_0.2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffe4d53760_0, 0, 1;
    %jmp T_0.5;
T_0.3 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffe4d53760_0, 0, 1;
    %jmp T_0.5;
T_0.5 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x7fffe4d7ed70;
T_1 ;
    %wait E_0x7fffe4d2ca70;
    %load/vec4 v0x7fffe4d7ef50_0;
    %dup/vec4;
    %pushi/vec4 1, 1, 2;
    %cmp/x;
    %jmp/1 T_1.0, 4;
    %dup/vec4;
    %pushi/vec4 3, 1, 2;
    %cmp/x;
    %jmp/1 T_1.1, 4;
    %pushi/vec4 1, 1, 1;
    %store/vec4 v0x7fffe4d7f030_0, 0, 1;
    %jmp T_1.3;
T_1.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffe4d7f030_0, 0, 1;
    %jmp T_1.3;
T_1.1 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffe4d7f030_0, 0, 1;
    %jmp T_1.3;
T_1.3 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x7fffe4d7f130;
T_2 ;
    %wait E_0x7fffe4d2ca70;
    %load/vec4 v0x7fffe4d7f310_0;
    %dup/vec4;
    %pushi/vec4 0, 1, 2;
    %cmp/z;
    %jmp/1 T_2.0, 4;
    %dup/vec4;
    %pushi/vec4 2, 1, 2;
    %cmp/z;
    %jmp/1 T_2.1, 4;
    %pushi/vec4 1, 1, 1;
    %store/vec4 v0x7fffe4d7f420_0, 0, 1;
    %jmp T_2.3;
T_2.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffe4d7f420_0, 0, 1;
    %jmp T_2.3;
T_2.1 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffe4d7f420_0, 0, 1;
    %jmp T_2.3;
T_2.3 ;
    %pop/vec4 1;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x7fffe4d66250;
T_3 ;
    %vpi_call 2 10 "$monitor", "Time=%0t | sel=%b | case=%b | casex=%b | casez=%b", $time, v0x7fffe4d7f540_0, v0x7fffe4d7f600_0, v0x7fffe4d7f6c0_0, v0x7fffe4d7f7c0_0 {0 0 0};
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x7fffe4d7f540_0, 0, 2;
    %delay 10, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x7fffe4d7f540_0, 0, 2;
    %delay 10, 0;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x7fffe4d7f540_0, 0, 2;
    %delay 10, 0;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x7fffe4d7f540_0, 0, 2;
    %delay 10, 0;
    %pushi/vec4 1, 1, 2;
    %store/vec4 v0x7fffe4d7f540_0, 0, 2;
    %delay 10, 0;
    %pushi/vec4 3, 1, 2;
    %store/vec4 v0x7fffe4d7f540_0, 0, 2;
    %delay 10, 0;
    %pushi/vec4 0, 2, 2;
    %store/vec4 v0x7fffe4d7f540_0, 0, 2;
    %delay 10, 0;
    %pushi/vec4 1, 2, 2;
    %store/vec4 v0x7fffe4d7f540_0, 0, 2;

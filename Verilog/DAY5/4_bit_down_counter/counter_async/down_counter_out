#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffb803db40 .scope module, "down_counter_tb" "down_counter_tb" 2 1;
 .timescale 0 0;
v0x7fffb8050720_0 .var "clk", 0 0;
v0x7fffb80507f0_0 .net "count", 3 0, v0x7fffb8050520_0;  1 drivers
v0x7fffb80508c0_0 .var "reset", 0 0;
S_0x7fffb803dcd0 .scope module, "uut" "down_counter" 2 7, 3 1 0, S_0x7fffb803db40;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 4 "count";
v0x7fffb803df20_0 .net "clk", 0 0, v0x7fffb8050720_0;  1 drivers
v0x7fffb8050520_0 .var "count", 3 0;
v0x7fffb8050600_0 .net "reset", 0 0, v0x7fffb80508c0_0;  1 drivers
E_0x7fffb8004b00 .event posedge, v0x7fffb8050600_0, v0x7fffb803df20_0;
    .scope S_0x7fffb803dcd0;
T_0 ;
    %wait E_0x7fffb8004b00;
    %load/vec4 v0x7fffb8050600_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 15, 0, 4;
    %assign/vec4 v0x7fffb8050520_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x7fffb8050520_0;
    %subi 1, 0, 4;
    %assign/vec4 v0x7fffb8050520_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x7fffb803db40;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffb8050720_0, 0, 1;
T_1.0 ;
    %delay 5, 0;
    %load/vec4 v0x7fffb8050720_0;
    %inv;
    %store/vec4 v0x7fffb8050720_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x7fffb803db40;
T_2 ;
    %vpi_call 2 21 "$dumpfile", "down_counter.vcd" {0 0 0};
    %vpi_call 2 22 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x7fffb803db40 {0 0 0};
    %vpi_call 2 24 "$monitor", "Time=%0t | reset=%b | count=%b", $time, v0x7fffb80508c0_0, v0x7fffb80507f0_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffb80508c0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffb80508c0_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 28 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "down_counter_tb.v";
    "down_counter.v";

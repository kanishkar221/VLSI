#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffc54ed560 .scope module, "toggle" "toggle" 2 1;
 .timescale 0 0;
v0x7fffc55375d0_0 .var "clk", 0 0;
v0x7fffc55376b0_0 .var/i "i", 31 0;
S_0x7fffc54ed6f0 .scope task, "toggle_signal" "toggle_signal" 2 5, 2 5 0, S_0x7fffc54ed560;
 .timescale 0 0;
v0x7fffc5514a90_0 .var "sig", 0 0;
TD_toggle.toggle_signal ;
    %load/vec4 v0x7fffc5514a90_0;
    %inv;
    %store/vec4 v0x7fffc5514a90_0, 0, 1;
    %delay 10, 0;
    %end;
    .scope S_0x7fffc54ed560;
T_1 ;
    %vpi_call 2 14 "$dumpfile", "toggle.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x7fffc54ed560 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffc55375d0_0, 0, 1;
    %vpi_call 2 18 "$monitor", "Time=%0t | clk=%b", $time, v0x7fffc55375d0_0 {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fffc55376b0_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x7fffc55376b0_0;
    %cmpi/s 10, 0, 32;
    %jmp/0xz T_1.1, 5;
    %load/vec4 v0x7fffc55375d0_0;
    %store/vec4 v0x7fffc5514a90_0, 0, 1;
    %fork TD_toggle.toggle_signal, S_0x7fffc54ed6f0;
    %join;
    %load/vec4 v0x7fffc5514a90_0;
    %store/vec4 v0x7fffc55375d0_0, 0, 1;
    %load/vec4 v0x7fffc55376b0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x7fffc55376b0_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %delay 10, 0;
    %vpi_call 2 24 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "toggle.v";

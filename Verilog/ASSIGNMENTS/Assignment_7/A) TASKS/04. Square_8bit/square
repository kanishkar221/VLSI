#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7ffff8a13570 .scope module, "square" "square" 2 1;
 .timescale 0 0;
v0x7ffff8a5f280_0 .var "num", 7 0;
v0x7ffff8a5f380_0 .var "square", 15 0;
S_0x7ffff8a13700 .scope task, "calc_square" "calc_square" 2 5, 2 5 0, S_0x7ffff8a13570;
 .timescale 0 0;
v0x7ffff8a3b490_0 .var "sq", 15 0;
v0x7ffff8a5f1a0_0 .var "x", 7 0;
TD_square.calc_square ;
    %load/vec4 v0x7ffff8a5f1a0_0;
    %pad/u 16;
    %load/vec4 v0x7ffff8a5f1a0_0;
    %pad/u 16;
    %mul;
    %store/vec4 v0x7ffff8a3b490_0, 0, 16;
    %end;
    .scope S_0x7ffff8a13570;
T_1 ;
    %vpi_call 2 14 "$monitor", "Time=%0t | num=%d | square=%d", $time, v0x7ffff8a5f280_0, v0x7ffff8a5f380_0 {0 0 0};
    %pushi/vec4 5, 0, 8;
    %store/vec4 v0x7ffff8a5f280_0, 0, 8;
    %load/vec4 v0x7ffff8a5f280_0;
    %store/vec4 v0x7ffff8a5f1a0_0, 0, 8;
    %fork TD_square.calc_square, S_0x7ffff8a13700;
    %join;
    %load/vec4 v0x7ffff8a3b490_0;
    %store/vec4 v0x7ffff8a5f380_0, 0, 16;
    %delay 10, 0;
    %pushi/vec4 10, 0, 8;
    %store/vec4 v0x7ffff8a5f280_0, 0, 8;
    %load/vec4 v0x7ffff8a5f280_0;
    %store/vec4 v0x7ffff8a5f1a0_0, 0, 8;
    %fork TD_square.calc_square, S_0x7ffff8a13700;
    %join;
    %load/vec4 v0x7ffff8a3b490_0;
    %store/vec4 v0x7ffff8a5f380_0, 0, 16;
    %delay 10, 0;
    %pushi/vec4 15, 0, 8;
    %store/vec4 v0x7ffff8a5f280_0, 0, 8;
    %load/vec4 v0x7ffff8a5f280_0;
    %store/vec4 v0x7ffff8a5f1a0_0, 0, 8;
    %fork TD_square.calc_square, S_0x7ffff8a13700;
    %join;
    %load/vec4 v0x7ffff8a3b490_0;
    %store/vec4 v0x7ffff8a5f380_0, 0, 16;
    %delay 10, 0;
    %pushi/vec4 20, 0, 8;
    %store/vec4 v0x7ffff8a5f280_0, 0, 8;
    %load/vec4 v0x7ffff8a5f280_0;
    %store/vec4 v0x7ffff8a5f1a0_0, 0, 8;
    %fork TD_square.calc_square, S_0x7ffff8a13700;
    %join;
    %load/vec4 v0x7ffff8a3b490_0;
    %store/vec4 v0x7ffff8a5f380_0, 0, 16;
    %delay 10, 0;
    %pushi/vec4 25, 0, 8;
    %store/vec4 v0x7ffff8a5f280_0, 0, 8;
    %load/vec4 v0x7ffff8a5f280_0;
    %store/vec4 v0x7ffff8a5f1a0_0, 0, 8;
    %fork TD_square.calc_square, S_0x7ffff8a13700;
    %join;
    %load/vec4 v0x7ffff8a3b490_0;
    %store/vec4 v0x7ffff8a5f380_0, 0, 16;
    %delay 50, 0;
    %vpi_call 2 22 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "square.v";

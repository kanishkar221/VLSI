v0x7fffdf13c9b0_0 .var "a", 3 0;
v0x7fffdf13cab0_0 .var "b", 3 0;
v0x7fffdf13cb90_0 .var "sum", 4 0;
S_0x7fffdf0ec620 .scope task, "serial_add_task" "serial_add_task" 2 6, 2 6 0, S_0x7fffdf0ec490;
 .timescale 0 0;
v0x7fffdf0ec800_0 .var "carry", 0 0;
v0x7fffdf13c520_0 .var/i "j", 31 0;
v0x7fffdf13c600_0 .var "result", 4 0;

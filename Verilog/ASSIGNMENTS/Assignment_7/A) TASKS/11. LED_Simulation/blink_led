#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffd5f7b590 .scope module, "blink_led" "blink_led" 2 1;
 .timescale 0 0;
v0x7fffd5fc6650_0 .var "led", 0 0;
S_0x7fffd5f7b720 .scope task, "blink_task" "blink_task" 2 4, 2 4 0, S_0x7fffd5f7b590;
 .timescale 0 0;
v0x7fffd5fa3320_0 .var/i "delay", 31 0;
v0x7fffd5fc64b0_0 .var/i "i", 31 0;
v0x7fffd5fc6590_0 .var/i "times", 31 0;
TD_blink_led.blink_task ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fffd5fc64b0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x7fffd5fc64b0_0;
    %load/vec4 v0x7fffd5fc6590_0;
    %cmp/s;
    %jmp/0xz T_0.1, 5;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffd5fc6650_0, 0, 1;
    %load/vec4 v0x7fffd5fa3320_0;
    %pad/s 64;
    %muli 1, 0, 64;
    %ix/vec4 4;
    %delayx 4;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffd5fc6650_0, 0, 1;
    %load/vec4 v0x7fffd5fa3320_0;
    %pad/s 64;
    %muli 1, 0, 64;
    %ix/vec4 4;
    %delayx 4;
    %load/vec4 v0x7fffd5fc64b0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x7fffd5fc64b0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .scope S_0x7fffd5f7b590;
T_1 ;
    %vpi_call 2 19 "$dumpfile", "blink_led.vcd" {0 0 0};
    %vpi_call 2 20 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x7fffd5f7b590 {0 0 0};
    %vpi_call 2 22 "$monitor", "Time=%0t | LED=%b", $time, v0x7fffd5fc6650_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffd5fc6650_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 5, 0, 32;
    %store/vec4 v0x7fffd5fc6590_0, 0, 32;
    %pushi/vec4 10, 0, 32;
    %store/vec4 v0x7fffd5fa3320_0, 0, 32;
    %fork TD_blink_led.blink_task, S_0x7fffd5f7b720;
    %join;
    %delay 100, 0;
    %vpi_call 2 27 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "blink_led.v";

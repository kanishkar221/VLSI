#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffdad3e570 .scope module, "monitor_signal" "monitor_signal" 2 1;
 .timescale 0 0;
v0x7fffdad3e700_0 .var "clk", 0 0;
v0x7fffdad877f0_0 .var "data", 0 0;
    .scope S_0x7fffdad3e570;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffdad3e700_0, 0, 1;
T_0.0 ;
    %delay 5, 0;
    %load/vec4 v0x7fffdad3e700_0;
    %inv;
    %store/vec4 v0x7fffdad3e700_0, 0, 1;
    %jmp T_0.0;
    %end;
    .thread T_0;
    .scope S_0x7fffdad3e570;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffdad877f0_0, 0, 1;
    %delay 12, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffdad877f0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffdad877f0_0, 0, 1;
    %delay 15, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffdad877f0_0, 0, 1;
    %delay 8, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffdad877f0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 15 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x7fffdad3e570;
T_2 ;
    %vpi_call 2 19 "$monitor", "Time=%0t clk=%b data=%b", $time, v0x7fffdad3e700_0, v0x7fffdad877f0_0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "monitor_signal.v";

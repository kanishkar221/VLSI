    %vpi_call 2 11 "$monitor", "%0t\011 %b %b | %b", $time, v0x7fffe687d030_0, v0x7fffe687d0f0_0, v0x7fffe687d1c0_0 {0 0 0};
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x7fffe687d030_0, 0, 4;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x7fffe687d0f0_0, 0, 4;

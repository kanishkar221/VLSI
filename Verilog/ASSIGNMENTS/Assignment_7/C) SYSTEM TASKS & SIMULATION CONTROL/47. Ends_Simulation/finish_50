#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffd857b540 .scope module, "finish_50" "finish_50" 2 1;
 .timescale 0 0;
v0x7fffd857b6d0_0 .var "clk", 0 0;
    .scope S_0x7fffd857b540;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffd857b6d0_0, 0, 1;
T_0.0 ;
    %delay 5, 0;
    %load/vec4 v0x7fffd857b6d0_0;
    %inv;
    %store/vec4 v0x7fffd857b6d0_0, 0, 1;
    %jmp T_0.0;
    %end;
    .thread T_0;
    .scope S_0x7fffd857b540;
T_1 ;
    %vpi_call 2 9 "$display", "Simulation Started .." {0 0 0};
    %delay 50, 0;
    %vpi_call 2 11 "$display", "Time=%0t Simulation Ending ", $time {0 0 0};
    %vpi_call 2 12 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "finish_50.v";

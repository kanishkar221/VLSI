#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffd5bb3240 .scope module, "tb_or_gate" "tb_or_gate" 2 1;
 .timescale 0 0;
v0x7fffd5bc5330_0 .var "a", 0 0;
v0x7fffd5bc5400_0 .var "b", 0 0;
v0x7fffd5bc54d0_0 .net "y", 0 0, L_0x7fffd5bc55d0;  1 drivers
S_0x7fffd5bb33d0 .scope module, "uut" "or_gate" 2 6, 3 1 0, S_0x7fffd5bb3240;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "y";
L_0x7fffd5bc55d0 .functor OR 1, v0x7fffd5bc5330_0, v0x7fffd5bc5400_0, C4<0>, C4<0>;
v0x7fffd5bb3600_0 .net "a", 0 0, v0x7fffd5bc5330_0;  1 drivers
v0x7fffd5bc5150_0 .net "b", 0 0, v0x7fffd5bc5400_0;  1 drivers
v0x7fffd5bc5210_0 .net "y", 0 0, L_0x7fffd5bc55d0;  alias, 1 drivers
    .scope S_0x7fffd5bb3240;
T_0 ;
    %vpi_call 2 9 "$monitor", "Time=%0t | a=%b |b=%b | y=%b", $time, v0x7fffd5bc5330_0, v0x7fffd5bc5400_0, v0x7fffd5bc54d0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffd5bc5330_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffd5bc5400_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffd5bc5330_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffd5bc5400_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffd5bc5330_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffd5bc5400_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffd5bc5330_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffd5bc5400_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 16 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_or_gate.v";
    "dataflow.v";

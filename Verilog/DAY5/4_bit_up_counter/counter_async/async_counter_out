#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffd0b2bb40 .scope module, "async_counter_tb" "async_counter_tb" 2 2;
 .timescale -9 -12;
v0x7fffd0b3e780_0 .var "clk", 0 0;
v0x7fffd0b3e850_0 .net "count", 3 0, v0x7fffd0b3e580_0;  1 drivers
v0x7fffd0b3e920_0 .var "reset", 0 0;
S_0x7fffd0b2bcd0 .scope module, "uut" "async_counter" 2 8, 3 1 0, S_0x7fffd0b2bb40;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 4 "count";
v0x7fffd0b2bf20_0 .net "clk", 0 0, v0x7fffd0b3e780_0;  1 drivers
v0x7fffd0b3e580_0 .var "count", 3 0;
v0x7fffd0b3e660_0 .net "reset", 0 0, v0x7fffd0b3e920_0;  1 drivers
E_0x7fffd0af2b00 .event posedge, v0x7fffd0b3e660_0, v0x7fffd0b2bf20_0;
    .scope S_0x7fffd0b2bcd0;
T_0 ;
    %wait E_0x7fffd0af2b00;
    %load/vec4 v0x7fffd0b3e660_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x7fffd0b3e580_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x7fffd0b3e580_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x7fffd0b3e580_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x7fffd0b2bb40;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffd0b3e780_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x7fffd0b3e780_0;
    %inv;
    %store/vec4 v0x7fffd0b3e780_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x7fffd0b2bb40;
T_2 ;
    %vpi_call 2 22 "$dumpfile", "async_counter.vcd" {0 0 0};
    %vpi_call 2 23 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x7fffd0b2bb40 {0 0 0};
    %vpi_call 2 25 "$monitor", "Time=%0t | reset=%b | count=%b", $time, v0x7fffd0b3e920_0, v0x7fffd0b3e850_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffd0b3e920_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffd0b3e920_0, 0, 1;
    %delay 100000, 0;
    %vpi_call 2 30 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "async_counter_tb.v";
    "async_counter.v";

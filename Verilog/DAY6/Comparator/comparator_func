#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffdcf94530 .scope module, "tb_comparator_func" "tb_comparator_func" 2 1;
 .timescale 0 0;
v0x7fffdcfab3e0_0 .var "a", 3 0;
v0x7fffdcfab4c0_0 .var "b", 3 0;
v0x7fffdcfab590_0 .net "eq", 0 0, v0x7fffdcfab0a0_0;  1 drivers
v0x7fffdcfab690_0 .net "gt", 0 0, v0x7fffdcfab170_0;  1 drivers
v0x7fffdcfab760_0 .net "lt", 0 0, v0x7fffdcfab230_0;  1 drivers
S_0x7fffdcf946c0 .scope task, "apply_stimulus" "apply_stimulus" 2 7, 2 7 0, S_0x7fffdcf94530;
 .timescale 0 0;
v0x7fffdcf948a0_0 .var "x", 3 0;
v0x7fffdcfaa6f0_0 .var "y", 3 0;
TD_tb_comparator_func.apply_stimulus ;
    %load/vec4 v0x7fffdcf948a0_0;
    %store/vec4 v0x7fffdcfab3e0_0, 0, 4;
    %load/vec4 v0x7fffdcfaa6f0_0;
    %store/vec4 v0x7fffdcfab4c0_0, 0, 4;
    %delay 5, 0;
    %vpi_call 2 12 "$display", "T=%0t | a=%0d, b=%0d ==> lt=%b eq=%b gt=%b", $time, v0x7fffdcfab3e0_0, v0x7fffdcfab4c0_0, v0x7fffdcfab760_0, v0x7fffdcfab590_0, v0x7fffdcfab690_0 {0 0 0};
    %end;
S_0x7fffdcfaa7d0 .scope module, "uut" "comparator_func" 2 5, 3 1 0, S_0x7fffdcf94530;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "a";
    .port_info 1 /INPUT 4 "b";
    .port_info 2 /OUTPUT 1 "lt";
    .port_info 3 /OUTPUT 1 "eq";
    .port_info 4 /OUTPUT 1 "gt";
v0x7fffdcfaaec0_0 .net "a", 3 0, v0x7fffdcfab3e0_0;  1 drivers
v0x7fffdcfaafc0_0 .net "b", 3 0, v0x7fffdcfab4c0_0;  1 drivers
v0x7fffdcfab0a0_0 .var "eq", 0 0;
v0x7fffdcfab170_0 .var "gt", 0 0;
v0x7fffdcfab230_0 .var "lt", 0 0;
E_0x7fffdcf923f0 .event anyedge, v0x7fffdcfaaec0_0, v0x7fffdcfaafc0_0;
S_0x7fffdcfaa9f0 .scope function.vec4.s3, "compare" "compare" 3 6, 3 6 0, S_0x7fffdcfaa7d0;
 .timescale 0 0;
; Variable compare is vec4 return value of scope S_0x7fffdcfaa9f0
v0x7fffdcfaacf0_0 .var "x", 3 0;
v0x7fffdcfaadd0_0 .var "y", 3 0;
TD_tb_comparator_func.uut.compare ;
    %load/vec4 v0x7fffdcfaacf0_0;
    %load/vec4 v0x7fffdcfaadd0_0;
    %cmp/u;
    %jmp/0xz  T_1.0, 5;
    %pushi/vec4 4, 0, 3;
    %ret/vec4 0, 0, 3;  Assign to compare (store_vec4_to_lval)
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x7fffdcfaacf0_0;
    %load/vec4 v0x7fffdcfaadd0_0;
    %cmp/e;
    %jmp/0xz  T_1.2, 4;
    %pushi/vec4 2, 0, 3;
    %ret/vec4 0, 0, 3;  Assign to compare (store_vec4_to_lval)
    %jmp T_1.3;
T_1.2 ;
    %pushi/vec4 1, 0, 3;
    %ret/vec4 0, 0, 3;  Assign to compare (store_vec4_to_lval)
T_1.3 ;
T_1.1 ;
    %end;
    .scope S_0x7fffdcfaa7d0;
T_2 ;
    %wait E_0x7fffdcf923f0;
    %load/vec4 v0x7fffdcfaaec0_0;
    %load/vec4 v0x7fffdcfaafc0_0;
    %store/vec4 v0x7fffdcfaadd0_0, 0, 4;
    %store/vec4 v0x7fffdcfaacf0_0, 0, 4;
    %callf/vec4 TD_tb_comparator_func.uut.compare, S_0x7fffdcfaa9f0;
    %split/vec4 1;
    %store/vec4 v0x7fffdcfab170_0, 0, 1;
    %split/vec4 1;
    %store/vec4 v0x7fffdcfab0a0_0, 0, 1;
    %store/vec4 v0x7fffdcfab230_0, 0, 1;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x7fffdcf94530;
T_3 ;
    %vpi_call 2 17 "$display", "---- Comparator Test ----" {0 0 0};
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x7fffdcf948a0_0, 0, 4;
    %pushi/vec4 7, 0, 4;
    %store/vec4 v0x7fffdcfaa6f0_0, 0, 4;
    %fork TD_tb_comparator_func.apply_stimulus, S_0x7fffdcf946c0;
    %join;
    %pushi/vec4 9, 0, 4;
    %store/vec4 v0x7fffdcf948a0_0, 0, 4;
    %pushi/vec4 9, 0, 4;
    %store/vec4 v0x7fffdcfaa6f0_0, 0, 4;
    %fork TD_tb_comparator_func.apply_stimulus, S_0x7fffdcf946c0;
    %join;
    %pushi/vec4 12, 0, 4;
    %store/vec4 v0x7fffdcf948a0_0, 0, 4;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x7fffdcfaa6f0_0, 0, 4;
    %fork TD_tb_comparator_func.apply_stimulus, S_0x7fffdcf946c0;
    %join;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x7fffdcf948a0_0, 0, 4;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x7fffdcfaa6f0_0, 0, 4;
    %fork TD_tb_comparator_func.apply_stimulus, S_0x7fffdcf946c0;
    %join;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x7fffdcf948a0_0, 0, 4;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x7fffdcfaa6f0_0, 0, 4;
    %fork TD_tb_comparator_func.apply_stimulus, S_0x7fffdcf946c0;
    %join;
    %delay 10, 0;
    %vpi_call 2 23 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;

S_0x7ffff1d07590 .scope module, "bit_reverse" "bit_reverse" 2 1;
 .timescale 0 0;
v0x7ffff1d547b0_0 .var "num", 7 0;
v0x7ffff1d548b0_0 .var "rev_num", 7 0;
S_0x7ffff1d07720 .scope function.vec4.s8, "reverse_bits" "reverse_bits" 2 6, 2 6 0, S_0x7ffff1d07590;
 .timescale 0 0;
v0x7ffff1d2fb90_0 .var/i "i", 31 0;
; Variable reverse_bits is vec4 return value of scope S_0x7ffff1d07720
v0x7ffff1d546f0_0 .var "x", 7 0;
TD_bit_reverse.reverse_bits ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7ffff1d2fb90_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x7ffff1d2fb90_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x7ffff1d546f0_0;
    %pushi/vec4 7, 0, 32;
    %load/vec4 v0x7ffff1d2fb90_0;
    %sub;
    %part/s 1;
    %ix/getv/s 4, v0x7ffff1d2fb90_0;
    %ret/vec4 0, 4, 1; Assign to reverse_bits (store_vec4_to_lval)
    %load/vec4 v0x7ffff1d2fb90_0;
    %addi 1, 0, 32;
    %store/vec4 v0x7ffff1d2fb90_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .scope S_0x7ffff1d07590;
T_1 ;
    %vpi_call 2 17 "$monitor", "Time=%0t | num=%b | reversed=%b", $time, v0x7ffff1d547b0_0, v0x7ffff1d548b0_0 {0 0 0};
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x7ffff1d547b0_0, 0, 8;
    %load/vec4 v0x7ffff1d547b0_0;
    %store/vec4 v0x7ffff1d546f0_0, 0, 8;
    %callf/vec4 TD_bit_reverse.reverse_bits, S_0x7ffff1d07720;
    %store/vec4 v0x7ffff1d548b0_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 176, 0, 8;
    %store/vec4 v0x7ffff1d547b0_0, 0, 8;
    %load/vec4 v0x7ffff1d547b0_0;
    %store/vec4 v0x7ffff1d546f0_0, 0, 8;
    %callf/vec4 TD_bit_reverse.reverse_bits, S_0x7ffff1d07720;
    %store/vec4 v0x7ffff1d548b0_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 240, 0, 8;
    %store/vec4 v0x7ffff1d547b0_0, 0, 8;
    %load/vec4 v0x7ffff1d547b0_0;
    %store/vec4 v0x7ffff1d546f0_0, 0, 8;
    %callf/vec4 TD_bit_reverse.reverse_bits, S_0x7ffff1d07720;
    %store/vec4 v0x7ffff1d548b0_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 210, 0, 8;
    %store/vec4 v0x7ffff1d547b0_0, 0, 8;
    %load/vec4 v0x7ffff1d547b0_0;
    %store/vec4 v0x7ffff1d546f0_0, 0, 8;
    %callf/vec4 TD_bit_reverse.reverse_bits, S_0x7ffff1d07720;
    %store/vec4 v0x7ffff1d548b0_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 128, 0, 8;
    %store/vec4 v0x7ffff1d547b0_0, 0, 8;

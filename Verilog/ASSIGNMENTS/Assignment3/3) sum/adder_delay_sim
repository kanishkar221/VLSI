 .timescale 0 0;
v0x7fffe687d030_0 .var "a", 3 0;
v0x7fffe687d0f0_0 .var "b", 3 0;
v0x7fffe687d1c0_0 .net "sum", 4 0, L_0x7fffe687d570;  1 drivers
S_0x7fffe68687c0 .scope module, "dut" "adder_delay" 2 5, 3 1 0, S_0x7fffe6868630;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "a";
    .port_info 1 /INPUT 4 "b";
    .port_info 2 /OUTPUT 5 "sum";
v0x7fffe682f490_0 .net *"_ivl_0", 4 0, L_0x7fffe687d2c0;  1 drivers
L_0x7f90ecee0018 .functor BUFT 1, C4<0>, C4<0>, C4<0>, C4<0>;
v0x7fffe687ca40_0 .net *"_ivl_3", 0 0, L_0x7f90ecee0018;  1 drivers

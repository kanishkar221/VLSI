#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffd9fa54f0 .scope module, "shift_right" "shift_right" 2 1;
 .timescale 0 0;
v0x7fffd9ff2f50_0 .var "n", 2 0;
v0x7fffd9ff3050_0 .var "num", 7 0;
v0x7fffd9ff3130_0 .var "shifted", 7 0;
S_0x7fffd9fa5680 .scope function.vec4.s8, "shift_right_n" "shift_right_n" 2 7, 2 7 0, S_0x7fffd9fa54f0;
 .timescale 0 0;
v0x7fffd9fa5860_0 .var "s", 2 0;
; Variable shift_right_n is vec4 return value of scope S_0x7fffd9fa5680
v0x7fffd9ff2e90_0 .var "x", 7 0;
TD_shift_right.shift_right_n ;
    %load/vec4 v0x7fffd9ff2e90_0;
    %ix/getv 4, v0x7fffd9fa5860_0;
    %shiftr 4;
    %ret/vec4 0, 0, 8;  Assign to shift_right_n (store_vec4_to_lval)
    %end;
    .scope S_0x7fffd9fa54f0;
T_1 ;
    %vpi_call 2 16 "$monitor", "Time=%0t | num=%b (%0d) | n=%0d | shifted=%b (%0d)", $time, v0x7fffd9ff3050_0, v0x7fffd9ff3050_0, v0x7fffd9ff2f50_0, v0x7fffd9ff3130_0, v0x7fffd9ff3130_0 {0 0 0};
    %pushi/vec4 240, 0, 8;
    %store/vec4 v0x7fffd9ff3050_0, 0, 8;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x7fffd9ff2f50_0, 0, 3;
    %load/vec4 v0x7fffd9ff3050_0;
    %load/vec4 v0x7fffd9ff2f50_0;
    %store/vec4 v0x7fffd9fa5860_0, 0, 3;
    %store/vec4 v0x7fffd9ff2e90_0, 0, 8;
    %callf/vec4 TD_shift_right.shift_right_n, S_0x7fffd9fa5680;
    %store/vec4 v0x7fffd9ff3130_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 240, 0, 8;
    %store/vec4 v0x7fffd9ff3050_0, 0, 8;
    %pushi/vec4 1, 0, 3;
    %store/vec4 v0x7fffd9ff2f50_0, 0, 3;
    %load/vec4 v0x7fffd9ff3050_0;
    %load/vec4 v0x7fffd9ff2f50_0;
    %store/vec4 v0x7fffd9fa5860_0, 0, 3;
    %store/vec4 v0x7fffd9ff2e90_0, 0, 8;
    %callf/vec4 TD_shift_right.shift_right_n, S_0x7fffd9fa5680;
    %store/vec4 v0x7fffd9ff3130_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 240, 0, 8;
    %store/vec4 v0x7fffd9ff3050_0, 0, 8;
    %pushi/vec4 2, 0, 3;
    %store/vec4 v0x7fffd9ff2f50_0, 0, 3;
    %load/vec4 v0x7fffd9ff3050_0;
    %load/vec4 v0x7fffd9ff2f50_0;
    %store/vec4 v0x7fffd9fa5860_0, 0, 3;
    %store/vec4 v0x7fffd9ff2e90_0, 0, 8;
    %callf/vec4 TD_shift_right.shift_right_n, S_0x7fffd9fa5680;
    %store/vec4 v0x7fffd9ff3130_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 240, 0, 8;
    %store/vec4 v0x7fffd9ff3050_0, 0, 8;
    %pushi/vec4 4, 0, 3;
    %store/vec4 v0x7fffd9ff2f50_0, 0, 3;
    %load/vec4 v0x7fffd9ff3050_0;
    %load/vec4 v0x7fffd9ff2f50_0;
    %store/vec4 v0x7fffd9fa5860_0, 0, 3;
    %store/vec4 v0x7fffd9ff2e90_0, 0, 8;
    %callf/vec4 TD_shift_right.shift_right_n, S_0x7fffd9fa5680;
    %store/vec4 v0x7fffd9ff3130_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 170, 0, 8;
    %store/vec4 v0x7fffd9ff3050_0, 0, 8;
    %pushi/vec4 3, 0, 3;
    %store/vec4 v0x7fffd9ff2f50_0, 0, 3;
    %load/vec4 v0x7fffd9ff3050_0;
    %load/vec4 v0x7fffd9ff2f50_0;
    %store/vec4 v0x7fffd9fa5860_0, 0, 3;
    %store/vec4 v0x7fffd9ff2e90_0, 0, 8;
    %callf/vec4 TD_shift_right.shift_right_n, S_0x7fffd9fa5680;
    %store/vec4 v0x7fffd9ff3130_0, 0, 8;
    %delay 10, 0;
    %delay 20, 0;
    %vpi_call 2 24 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.

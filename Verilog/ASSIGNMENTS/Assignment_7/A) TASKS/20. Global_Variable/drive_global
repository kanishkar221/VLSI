#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffc39ee570 .scope module, "drive_global" "drive_global" 2 1;
 .timescale 0 0;
v0x7fffc3a399b0_0 .var "a", 7 0;
v0x7fffc3a39ab0_0 .var "b", 7 0;
v0x7fffc3a39b90_0 .var "global_sum", 8 0;
S_0x7fffc39ee700 .scope task, "drive_sum_task" "drive_sum_task" 2 5, 2 5 0, S_0x7fffc39ee570;
 .timescale 0 0;
v0x7fffc3a15fb0_0 .var "x", 7 0;
v0x7fffc3a398d0_0 .var "y", 7 0;
TD_drive_global.drive_sum_task ;
    %load/vec4 v0x7fffc3a15fb0_0;
    %pad/u 9;
    %load/vec4 v0x7fffc3a398d0_0;
    %pad/u 9;
    %add;
    %store/vec4 v0x7fffc3a39b90_0, 0, 9;
    %end;
    .scope S_0x7fffc39ee570;
T_1 ;
    %vpi_call 2 13 "$monitor", "Time=%0t | a=%d b=%d | global_sum=%d", $time, v0x7fffc3a399b0_0, v0x7fffc3a39ab0_0, v0x7fffc3a39b90_0 {0 0 0};
    %pushi/vec4 10, 0, 8;
    %store/vec4 v0x7fffc3a399b0_0, 0, 8;
    %pushi/vec4 20, 0, 8;
    %store/vec4 v0x7fffc3a39ab0_0, 0, 8;
    %load/vec4 v0x7fffc3a399b0_0;
    %store/vec4 v0x7fffc3a15fb0_0, 0, 8;
    %load/vec4 v0x7fffc3a39ab0_0;
    %store/vec4 v0x7fffc3a398d0_0, 0, 8;
    %fork TD_drive_global.drive_sum_task, S_0x7fffc39ee700;
    %join;
    %delay 10, 0;
    %pushi/vec4 50, 0, 8;
    %store/vec4 v0x7fffc3a399b0_0, 0, 8;
    %pushi/vec4 25, 0, 8;
    %store/vec4 v0x7fffc3a39ab0_0, 0, 8;
    %load/vec4 v0x7fffc3a399b0_0;
    %store/vec4 v0x7fffc3a15fb0_0, 0, 8;
    %load/vec4 v0x7fffc3a39ab0_0;
    %store/vec4 v0x7fffc3a398d0_0, 0, 8;
    %fork TD_drive_global.drive_sum_task, S_0x7fffc39ee700;
    %join;
    %delay 10, 0;
    %pushi/vec4 100, 0, 8;
    %store/vec4 v0x7fffc3a399b0_0, 0, 8;
    %pushi/vec4 50, 0, 8;
    %store/vec4 v0x7fffc3a39ab0_0, 0, 8;
    %load/vec4 v0x7fffc3a399b0_0;
    %store/vec4 v0x7fffc3a15fb0_0, 0, 8;
    %load/vec4 v0x7fffc3a39ab0_0;
    %store/vec4 v0x7fffc3a398d0_0, 0, 8;
    %fork TD_drive_global.drive_sum_task, S_0x7fffc39ee700;
    %join;
    %delay 10, 0;
    %delay 20, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "drive_global.v";

#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffcd054cb0 .scope module, "ring_counter_tb" "ring_counter_tb" 2 1;
 .timescale 0 0;
v0x7fffcd067940_0 .var "clk", 0 0;
v0x7fffcd067a10_0 .net "q", 3 0, v0x7fffcd067740_0;  1 drivers
v0x7fffcd067ae0_0 .var "reset", 0 0;
S_0x7fffcd054e40 .scope module, "uut" "ring_counter" 2 6, 3 1 0, S_0x7fffcd054cb0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 4 "q";
v0x7fffcd055090_0 .net "clk", 0 0, v0x7fffcd067940_0;  1 drivers
v0x7fffcd067740_0 .var "q", 3 0;
v0x7fffcd067820_0 .net "reset", 0 0, v0x7fffcd067ae0_0;  1 drivers
E_0x7fffcd01bb00 .event posedge, v0x7fffcd055090_0;
    .scope S_0x7fffcd054e40;
T_0 ;
    %wait E_0x7fffcd01bb00;
    %load/vec4 v0x7fffcd067820_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 8, 0, 4;
    %assign/vec4 v0x7fffcd067740_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x7fffcd067740_0;
    %parti/s 3, 0, 2;
    %load/vec4 v0x7fffcd067740_0;
    %parti/s 1, 3, 3;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x7fffcd067740_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x7fffcd054cb0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffcd067940_0, 0, 1;
T_1.0 ;
    %delay 5, 0;
    %load/vec4 v0x7fffcd067940_0;
    %inv;
    %store/vec4 v0x7fffcd067940_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x7fffcd054cb0;
T_2 ;
    %vpi_call 2 18 "$dumpfile", "ring_counter.vcd" {0 0 0};
    %vpi_call 2 19 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x7fffcd054cb0 {0 0 0};
    %vpi_call 2 21 "$monitor", "Time=%0t | reset=%b | q=%b", $time, v0x7fffcd067ae0_0, v0x7fffcd067a10_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffcd067ae0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffcd067ae0_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 27 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "ring_counter_tb.v";
    "ring_counter.v";

#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffc14df540 .scope module, "real_float" "real_float" 2 2;
 .timescale -9 -12;
v0x7fffc14df6d0_0 .var/real "current_time", 0 0;
    .scope S_0x7fffc14df540;
T_0 ;
    %vpi_call 2 5 "$monitor", "Time=%0t  : current_time=%.3f", $time, v0x7fffc14df6d0_0 {0 0 0};
    %vpi_func/r 2 6 "$realtime" {0 0 0};
    %store/real v0x7fffc14df6d0_0;
    %delay 10000, 0;
    %vpi_func/r 2 7 "$realtime" {0 0 0};
    %store/real v0x7fffc14df6d0_0;
    %delay 20000, 0;
    %vpi_func/r 2 8 "$realtime" {0 0 0};
    %store/real v0x7fffc14df6d0_0;
    %delay 30000, 0;
    %vpi_call 2 9 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "real_float.v";

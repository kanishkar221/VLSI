L_0x7f7a79cb0060 .functor BUFT 1, C4<0>, C4<0>, C4<0>, C4<0>;
v0x7fffd30ffc70_0 .net *"_ivl_7", 0 0, L_0x7f7a79cb0060;  1 drivers
v0x7fffd30ffd50_0 .var "a", 3 0;
v0x7fffd30ffe80_0 .var "b", 3 0;
v0x7fffd30fff60_0 .net "sum", 4 0, L_0x7fffd3100300;  1 drivers
L_0x7fffd3100040 .concat [ 4 1 0 0], v0x7fffd30ffd50_0, L_0x7f7a79cb0018;
L_0x7fffd31001b0 .concat [ 4 1 0 0], v0x7fffd30ffe80_0, L_0x7f7a79cb0060;
L_0x7fffd3100300 .arith/sum 5, L_0x7fffd3100040, L_0x7fffd31001b0;
    .scope S_0x7fffd30b7510;
T_0 ;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x7fffd30ffd50_0, 0, 4;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x7fffd30ffe80_0, 0, 4;
    %vpi_call 2 10 "$display", "A = %d, B = %d, SUM = %d", v0x7fffd30ffd50_0, v0x7fffd30ffe80_0, v0x7fffd30fff60_0 {0 0 0};
    %vpi_call 2 11 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.

#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffd0dc5590 .scope module, "add8" "add8" 2 1;
 .timescale 0 0;
v0x7fffd0e13500_0 .var "a", 7 0;
v0x7fffd0e13600_0 .var "b", 7 0;
v0x7fffd0e136e0_0 .var/i "i", 31 0;
v0x7fffd0e137d0_0 .var "sum_task", 8 0;
S_0x7fffd0dc5720 .scope task, "add8_task" "add8_task" 2 6, 2 6 0, S_0x7fffd0dc5590;
 .timescale 0 0;
v0x7fffd0dee2e0_0 .var "s", 8 0;
v0x7fffd0e13360_0 .var "x", 7 0;
v0x7fffd0e13440_0 .var "y", 7 0;
TD_add8.add8_task ;
    %load/vec4 v0x7fffd0e13360_0;
    %pad/u 9;
    %load/vec4 v0x7fffd0e13440_0;
    %pad/u 9;
    %add;
    %store/vec4 v0x7fffd0dee2e0_0, 0, 9;
    %end;
    .scope S_0x7fffd0dc5590;
T_1 ;
    %vpi_call 2 16 "$dumpfile", "add8.vcd" {0 0 0};
    %vpi_call 2 17 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x7fffd0dc5590 {0 0 0};
    %vpi_call 2 19 "$monitor", "Time=%0t : a=%d b=%d sum=%d carry=%b", $time, v0x7fffd0e13500_0, v0x7fffd0e13600_0, &PV<v0x7fffd0e137d0_0, 0, 8>, &PV<v0x7fffd0e137d0_0, 8, 1> {0 0 0};
    %pushi/vec4 10, 0, 8;
    %store/vec4 v0x7fffd0e13500_0, 0, 8;
    %pushi/vec4 20, 0, 8;
    %store/vec4 v0x7fffd0e13600_0, 0, 8;
    %delay 1, 0;
    %load/vec4 v0x7fffd0e13500_0;
    %store/vec4 v0x7fffd0e13360_0, 0, 8;
    %load/vec4 v0x7fffd0e13600_0;
    %store/vec4 v0x7fffd0e13440_0, 0, 8;
    %fork TD_add8.add8_task, S_0x7fffd0dc5720;
    %join;
    %load/vec4 v0x7fffd0dee2e0_0;
    %store/vec4 v0x7fffd0e137d0_0, 0, 9;
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0x7fffd0e13500_0, 0, 8;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x7fffd0e13600_0, 0, 8;
    %delay 1, 0;
    %load/vec4 v0x7fffd0e13500_0;
    %store/vec4 v0x7fffd0e13360_0, 0, 8;
    %load/vec4 v0x7fffd0e13600_0;
    %store/vec4 v0x7fffd0e13440_0, 0, 8;
    %fork TD_add8.add8_task, S_0x7fffd0dc5720;
    %join;
    %load/vec4 v0x7fffd0dee2e0_0;
    %store/vec4 v0x7fffd0e137d0_0, 0, 9;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fffd0e136e0_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x7fffd0e136e0_0;
    %cmpi/s 4, 0, 32;
    %jmp/0xz T_1.1, 5;
    %vpi_func 2 27 "$random" 32 {0 0 0};
    %pad/s 8;
    %store/vec4 v0x7fffd0e13500_0, 0, 8;
    %vpi_func 2 28 "$random" 32 {0 0 0};
    %pad/s 8;
    %store/vec4 v0x7fffd0e13600_0, 0, 8;
    %delay 1, 0;
    %load/vec4 v0x7fffd0e13500_0;
    %store/vec4 v0x7fffd0e13360_0, 0, 8;
    %load/vec4 v0x7fffd0e13600_0;
    %store/vec4 v0x7fffd0e13440_0, 0, 8;
    %fork TD_add8.add8_task, S_0x7fffd0dc5720;
    %join;
    %load/vec4 v0x7fffd0dee2e0_0;
    %store/vec4 v0x7fffd0e137d0_0, 0, 9;
    %load/vec4 v0x7fffd0e136e0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x7fffd0e136e0_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %delay 1, 0;
    %vpi_call 2 32 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.

#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x7fffe2807690 .scope module, "factorial_tb" "factorial_tb" 2 1;
 .timescale 0 0;
v0x7fffe281a2f0_0 .var "num", 7 0;
v0x7fffe281a3b0_0 .net "res", 15 0, L_0x7fffe281a480;  1 drivers
S_0x7fffe2807820 .scope module, "uut" "factorial" 2 5, 3 1 0, S_0x7fffe2807690;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "n";
    .port_info 1 /OUTPUT 16 "result";
v0x7fffe281a0b0_0 .net "n", 7 0, v0x7fffe281a2f0_0;  1 drivers
v0x7fffe281a1b0_0 .net "result", 15 0, L_0x7fffe281a480;  alias, 1 drivers
L_0x7fffe281a480 .ufunc/vec4 TD_factorial_tb.uut.fact, 16, v0x7fffe281a2f0_0 (v0x7fffe2808100_0) S_0x7fffe27cd490;
S_0x7fffe27cd490 .scope autofunction.vec4.s16, "fact" "fact" 3 7, 3 7 0, S_0x7fffe2807820;
 .timescale 0 0;
v0x7fffe2808100_0 .var "N", 7 0;
; Variable fact is vec4 return value of scope S_0x7fffe27cd490
TD_factorial_tb.uut.fact ;
    %load/vec4 v0x7fffe2808100_0;
    %pad/u 32;
    %cmpi/u 1, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz  T_0.0, 5;
    %pushi/vec4 1, 0, 16;
    %ret/vec4 0, 0, 16;  Assign to fact (store_vec4_to_lval)
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x7fffe2808100_0;
    %pad/u 16;
    %alloc S_0x7fffe27cd490;
    %load/vec4 v0x7fffe2808100_0;
    %subi 1, 0, 8;
    %store/vec4 v0x7fffe2808100_0, 0, 8;
    %callf/vec4 TD_factorial_tb.uut.fact, S_0x7fffe27cd490;
    %free S_0x7fffe27cd490;
    %mul;
    %ret/vec4 0, 0, 16;  Assign to fact (store_vec4_to_lval)
T_0.1 ;
    %end;
    .scope S_0x7fffe2807690;
T_1 ;
    %vpi_call 2 11 "$monitor", "Time=%0t | num=%0d | factorial=%0d", $time, v0x7fffe281a2f0_0, v0x7fffe281a3b0_0 {0 0 0};
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x7fffe281a2f0_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x7fffe281a2f0_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 2, 0, 8;
    %store/vec4 v0x7fffe281a2f0_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 3, 0, 8;
    %store/vec4 v0x7fffe281a2f0_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 4, 0, 8;
    %store/vec4 v0x7fffe281a2f0_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 5, 0, 8;
    %store/vec4 v0x7fffe281a2f0_0, 0, 8;
    %delay 10, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
